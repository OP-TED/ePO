   <sh:NodeShape rdf:about="http://unknown.domain/for/prefix#CompetitionNotice">
      <sh:targetClass rdf:resource="http://unknown.domain/for/prefix#CompetitionNotice"/>
      <sh:name>Competition notice</sh:name>
      <rdfs:label xml:lang="en">Competition notice</rdfs:label>
      <sh:description>An announcement of the launch of a procurement procedure by a procuring entity.

(WG approval 18/05/2018)
</sh:description>
      <rdfs:comment xml:lang="en">An announcement of the launch of a procurement procedure by a procuring entity.

(WG approval 18/05/2018)
</rdfs:comment>
   </sh:NodeShape>
