      <sh:name>Completion notice</sh:name>
      <rdfs:label xml:lang="en">Completion notice</rdfs:label>
      <sh:description>An announcement of the end of a procurement by a buyer.</sh:description>
      <rdfs:comment xml:lang="en">An announcement of the end of a procurement by a buyer.</rdfs:comment>
   </sh:NodeShape>
   <sh:NodeShape rdf:about="http://unknown.domain/for/prefix#ContractModificationNotice">
