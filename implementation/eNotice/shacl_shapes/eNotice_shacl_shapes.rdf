      <sh:name>Contract modification notice</sh:name>
      <rdfs:label xml:lang="en">Contract modification notice</rdfs:label>
      <sh:description>An announcement of the modification of a contract/concession during during its term by a procuring entity.</sh:description>
      <rdfs:comment xml:lang="en">An announcement of the modification of a contract/concession during during its term by a procuring entity.</rdfs:comment>
   </sh:NodeShape>
   <sh:NodeShape rdf:about="http://unknown.domain/for/prefix#DirectAwardPrenotificationNotice">
      <sh:targetClass rdf:resource="http://unknown.domain/for/prefix#DirectAwardPrenotificationNotice"/>
      <sh:name>Direct award prenotification notice</sh:name>
      <rdfs:label xml:lang="en">Direct award prenotification notice</rdfs:label>
      <sh:description>Notice which sets out the buyer's purchasing intention to award a contract without prior notification of competition.</sh:description>
      <rdfs:comment xml:lang="en">Notice which sets out the buyer's purchasing intention to award a contract without prior notification of competition.</rdfs:comment>
   </sh:NodeShape>
   <sh:NodeShape rdf:about="http://unknown.domain/for/prefix#PlanningNotice">
      <sh:targetClass rdf:resource="http://unknown.domain/for/prefix#PlanningNotice"/>
      <sh:name>Planning notice</sh:name>
      <rdfs:label xml:lang="en">Planning notice</rdfs:label>
      <sh:description>Notice which sets out the contracting authority's purchasing intentions. It is used by contracting authorities to provide suppliers with information about a procurement process. 
</sh:description>
      <rdfs:comment xml:lang="en">Notice which sets out the contracting authority's purchasing intentions. It is used by contracting authorities to provide suppliers with information about a procurement process. 
</rdfs:comment>
