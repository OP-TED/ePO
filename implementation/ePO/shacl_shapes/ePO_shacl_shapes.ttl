epo:AccessTerm a sh:NodeShape ;
    rdfs:label "Access term"@en ;
    rdfs:comment """Conditions and stipulations about where and how to access the procurement documents.

WG Approval 09/03/2021

"""@en ;
    rdfs:isDefinedBy <http://data.europa.eu/a4g/ontology> ;
    sh:description """Conditions and stipulations about where and how to access the procurement documents.

WG Approval 09/03/2021

""" ;
    sh:name "Access term" ;
    sh:property [ a sh:PropertyShape ;
            sh:datatype xsd:boolean ;
            sh:name "Is procurement document restricted" ;
            sh:path epo:isProcurementDocumentRestricted ],
        [ a sh:PropertyShape ;
            sh:class epo:CatalogueProvider ;
            sh:name "Defines catalogue provider" ;
            sh:path epo:definesCatalogueProvider ],
        [ a sh:PropertyShape ;
            sh:class epo:ProcurementProcedureInformationProvider ;
            sh:name "Defines procurement procedure information provider" ;
            sh:path epo:definesProcurementProcedureInformationProvider ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Defines offline access provider" ;
            sh:path epo:definesOfflineAccessProvider ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has document restriction justification" ;
            sh:path epo:hasDocumentRestrictionJustification ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has restricted access u r l" ;
            sh:path epo:hasRestrictedAccessURL ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:anyURI ;
            sh:name "Has restricted access u r l" ;
            sh:path epo:hasRestrictedAccessURL ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:anyURI ;
            sh:name "Has public access u r l" ;
            sh:path epo:hasPublicAccessURL ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Defines procurement procedure information provider" ;
            sh:path epo:definesProcurementProcedureInformationProvider ],
        [ a sh:PropertyShape ;
            sh:class epo:PlannedProcurementPart ;
            sh:name "Refers to planned part" ;
            sh:path epo:refersToPlannedPart ],
        [ a sh:PropertyShape ;
            sh:class epo:OfflineAccessProvider ;
            sh:name "Defines offline access provider" ;
            sh:path epo:definesOfflineAccessProvider ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:dateTime ;
            sh:name "Has additional information deadline" ;
            sh:path epo:hasAdditionalInformationDeadline ],
        [ a sh:PropertyShape ;
            sh:class epo:ProcurementDocument ;
            sh:name "Involves procurement document" ;
            sh:path epo:involvesProcurementDocument ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Involves procurement document" ;
            sh:path epo:involvesProcurementDocument ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has additional information deadline" ;
            sh:path epo:hasAdditionalInformationDeadline ],
        [ a sh:PropertyShape ;
            sh:class epo:CatalogueReceiver ;
            sh:name "Defines catalogue receiver" ;
            sh:path epo:definesCatalogueReceiver ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has public access u r l" ;
            sh:path epo:hasPublicAccessURL ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Is procurement document restricted" ;
            sh:path epo:isProcurementDocumentRestricted ] ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#definesOfflineAccessProvider> ?that . ?that <http://data.europa.eu/a4g/ontology#definesOfflineAccessProvider> ?this .} " ],
        [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasDocumentRestrictionJustification> ?that . ?that <http://data.europa.eu/a4g/ontology#hasDocumentRestrictionJustification> ?this .} " ],
        [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#definesCatalogueProvider> ?that . ?that <http://data.europa.eu/a4g/ontology#definesCatalogueProvider> ?this .} " ],
        [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#definesProcurementProcedureInformationProvider> ?that . ?that <http://data.europa.eu/a4g/ontology#definesProcurementProcedureInformationProvider> ?this .} " ],
        [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#definesCatalogueReceiver> ?that . ?that <http://data.europa.eu/a4g/ontology#definesCatalogueReceiver> ?this .} " ],
        [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#refersToPlannedPart> ?that . ?that <http://data.europa.eu/a4g/ontology#refersToPlannedPart> ?this .} " ],
        [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#involvesProcurementDocument> ?that . ?that <http://data.europa.eu/a4g/ontology#involvesProcurementDocument> ?this .} " ] ;
    sh:targetClass epo:AccessTerm .

