    owl:versionInfo "3.1.0" .

epo:Batch a sh:NodeShape ;
    rdfs:label "Batch"@en ;
    rdfs:comment """A production Lot in which the Item instances were produced. 

WG Acceptance 10/03/2022
"""@en ;
    rdfs:isDefinedBy <http://data.europa.eu/a4g/ontology> ;
    sh:description """A production Lot in which the Item instances were produced. 

WG Acceptance 10/03/2022
""" ;
    sh:name "Batch" ;
    sh:property [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has expiry date" ;
            sh:path epo:hasExpiryDate ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has best before date" ;
            sh:path epo:hasBestBeforeDate ],
        [ a sh:PropertyShape ;
            sh:class epo:Item ;
            sh:name "Is production of" ;
            sh:path epo:isProductionOf ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:dateTime ;
            sh:name "Has manufacture date" ;
            sh:path epo:hasManufactureDate ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has manufacture date" ;
            sh:path epo:hasManufactureDate ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:date ;
            sh:name "Has best before date" ;
            sh:path epo:hasBestBeforeDate ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Is production of" ;
            sh:path epo:isProductionOf ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:date ;
            sh:name "Has expiry date" ;
            sh:path epo:hasExpiryDate ] ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#isProductionOf> ?that . ?that <http://data.europa.eu/a4g/ontology#isProductionOf> ?this .} " ] ;
    sh:targetClass epo:Batch .

