    dc:contributor [ a foaf:Person ;
            foaf:homepage <http://costezki.ro> ;
            foaf:name "Eugeniu" ;
            foaf:surname "Costetchi" ;
            schema:affiliation [ foaf:name "Meaningfy" ;
                    foaf:page <http://meaningfy.ws> ] ],
        [ a foaf:Person ;
            foaf:name "Andreea" ;
            foaf:surname "Pasare" ;
