        <http://data.europa.eu/a4g/data-shape#epo-cat-Item-epo-cat-hasDimensionDescription>,
        <http://data.europa.eu/a4g/data-shape#epo-cat-Item-epo-cat-hasHazardousClass>,
        <http://data.europa.eu/a4g/data-shape#epo-cat-Item-epo-cat-hasKeyword>,
        <http://data.europa.eu/a4g/data-shape#epo-cat-Item-epo-cat-isInStock> ;
    sh:targetClass :Item .

