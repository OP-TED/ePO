            sh:path epo:hasTargetCatalogueVersion ] ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#announcesCatalogueFragment> ?that . ?that <http://data.europa.eu/a4g/ontology#announcesCatalogueFragment> ?this .} " ] ;
    sh:targetClass epo:CatalogueUpdateDocument .

