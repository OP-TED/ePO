@prefix : <http://data.europa.eu/a4g/ontology#> .
@prefix at-voc: <http://publications.europa.eu/resource/authority/> .
@prefix at-voc-new: <http://publications.europa.eu/resource/authority/new/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix cat-shape: <http://data.europa.eu/a4g/data-shape#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix cccev: <http://data.europa.eu/m8g/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

cat-shape:at-voc-new-dangerous-goods-list rdfs:comment """Will this ever be a controlled vocabulary? 

To be proposed to EUVocabularies for publication. 
""" .

cat-shape:at-voc-new-orderable-unit rdfs:comment "Waiting for code list." .

cat-shape:at-voc-new-transaction-conditions rdfs:comment "Waiting for code list." .

cat-shape:epo-cat-Batch a sh:NodeShape ;
    rdfs:label "Batch" ;
    rdfs:comment """A production Lot in which the Item instances were produced. 

WG Acceptance 10/03/2022
""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-Batch-epo-cat-hasBestBeforeDate,
        cat-shape:epo-cat-Batch-epo-cat-hasExpiryDate,
        cat-shape:epo-cat-Batch-epo-cat-hasManufactureDate,
        cat-shape:epo-cat-Batch-epo-cat-isProductionOf ;
    sh:targetClass :Batch .

cat-shape:epo-cat-Brand a sh:NodeShape ;
    rdfs:label "Brand" ;
    rdfs:comment """Name given by an organisation to its product or service, or range of products or services. 

Additional information:
Brand name allows economic operators to distinctively describe an item in their eCatalogue. This may be different from the "item name".

WG acceptance 10/03/2022
""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-Brand-dct-title ;
    sh:targetClass :Brand .

cat-shape:epo-cat-Catalogue a sh:NodeShape ;
    rdfs:label "Catalogue" ;
    rdfs:comment """A Document describing a set of Items offered for purchase that can be processed in an electronic way.
""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-Catalogue-epo-cat-comprisesCatalogueLine,
        cat-shape:epo-cat-Catalogue-epo-cat-hasActivityCode,
        cat-shape:epo-cat-Catalogue-epo-cat-hasSpecificPaymentArrangement ;
    sh:targetClass :Catalogue .

cat-shape:epo-cat-CatalogueLine a sh:NodeShape ;
    rdfs:label "Catalogue line" ;
    rdfs:comment """Aggregated information about the Terms and specifications for procuring an Item.


Details about an Item, like Prices, packaging, discounts, special promotions, etc.


TC 440:

Detailed information of an Item, Price and its composition in a Catalogue.

Composition: e.g. information referring to its description, how to order it, what it is made of, properties, delivery conditions, etc.





""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-CatalogueLine-epo-cat-hasActivityCode,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasChargeInformation,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasOrderableQuantityIncrement,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasOrderableUnit,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasOrderableUnitFactorRate,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasPrice,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasStandardisedUnitPrice,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasTransactionConditions,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasTransactionConditionsCode,
        cat-shape:epo-cat-CatalogueLine-epo-cat-hasWarrantyInformation,
        cat-shape:epo-cat-CatalogueLine-epo-cat-isContractedItem,
        cat-shape:epo-cat-CatalogueLine-epo-cat-isOrderable ;
    sh:targetClass :CatalogueLine .

cat-shape:epo-cat-ChargeInformation a sh:NodeShape ;
    rdfs:label "Charge information" ;
    rdfs:comment """Information about tax, fee or duty imposed.

Additional Information:
Charge category indicates the nature of the tax/duty/fee, for example VAT, CAR, etc.
Charge category modifier may be used in case different levels, exemptions or other modifications apply.
The charge can be fixed or relative to the price.

WG approval 16/05/2023""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :ChargeInformation .

cat-shape:epo-cat-InformationHub a sh:NodeShape ;
    rdfs:label "Information hub" ;
    rdfs:comment """Grouping of data that may be associated to various concepts.

Additional Information:
For example, AllowanceChargeInformation may be associated to the Order or the Catalogue (either at the Line level or at the Document level), amongst others.

WG approval 16/05/2023""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-InformationHub-epo-cat-isSpecificToLine ;
    sh:targetClass :InformationHub .

cat-shape:epo-cat-Item a sh:NodeShape ;
    rdfs:label "Item" ;
    rdfs:comment """A product, work, service or any combination of them.

Additional Information:
An Item can be an atomic Thing or a composition of things that together are seen as a unit, e.g. a tetrabrik of milk or an indivisible package of six tetrabriks.

No distinction is provided between services, goods and works to leave room for innovation.

WG approval 30/06/2022""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-Item-dct-description,
        cat-shape:epo-cat-Item-dct-hasPart,
        cat-shape:epo-cat-Item-dct-isReplacedBy,
        cat-shape:epo-cat-Item-dct-replaces,
        cat-shape:epo-cat-Item-dct-requires,
        cat-shape:epo-cat-Item-dct-title,
        cat-shape:epo-cat-Item-epo-cat-hasAccessoryItem,
        cat-shape:epo-cat-Item-epo-cat-hasBrand,
        cat-shape:epo-cat-Item-epo-cat-hasComplementaryItem,
        cat-shape:epo-cat-Item-epo-cat-hasDeliveryClassification,
        cat-shape:epo-cat-Item-epo-cat-hasDimensionDescription,
        cat-shape:epo-cat-Item-epo-cat-hasHazardousClass,
        cat-shape:epo-cat-Item-epo-cat-hasHazardousItemUNDGCode,
        cat-shape:epo-cat-Item-epo-cat-hasItemClassification,
        cat-shape:epo-cat-Item-epo-cat-hasKeyword,
        cat-shape:epo-cat-Item-epo-cat-hasManufacturer,
        cat-shape:epo-cat-Item-epo-cat-hasModel,
        cat-shape:epo-cat-Item-epo-cat-hasQualifiedItemRelation,
        cat-shape:epo-cat-Item-epo-cat-hasSpecification,
        cat-shape:epo-cat-Item-epo-cat-isInStock ;
    sh:targetClass :Item .

cat-shape:epo-cat-ItemAccessoryRelation a sh:NodeShape ;
    rdfs:label "Item accessory relation" ;
    rdfs:comment """Combination of two Items where the former enhances the latter.

WG Approval: 30/06/2022
""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :ItemAccessoryRelation .

cat-shape:epo-cat-ItemComplementaryRelation a sh:NodeShape ;
    rdfs:label "Item complementary relation" ;
    rdfs:comment """Combination of two Items which mutually enhance one another.

WG Approval: 30/06/2022

""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :ItemComplementaryRelation .

cat-shape:epo-cat-ItemComposition a sh:NodeShape ;
    rdfs:label "Item composition" ;
    rdfs:comment """Inclusion of an Item into in the described Item.

Additional Information:
The Item is provided with its components, however, the component Items may also be provided separately. 
For example, a bicycle has wheels as components, but they may be provided as spare parts as well. 
This relation is useful to describe different package levels of an item or hierarchies of Items.

WG Approval: 19/05/2022


""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :ItemComposition .

cat-shape:epo-cat-ItemModel a sh:NodeShape ;
    rdfs:label "Item model" ;
    rdfs:comment """A specific design or version of a product or service.

Additional information:
This class is meant as a reusable reference to the model name rather than as a set of specifications of an Item of this particular model. 

WG acceptance 10/03/2022

""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-ItemModel-dct-title ;
    sh:targetClass :ItemModel .

cat-shape:epo-cat-ItemProperty a sh:NodeShape ;
    rdfs:label "Item property" ;
    rdfs:comment """Construct meant to represent the association between an attribute defined in an external classification scheme and the value ascribed to it.

WG approval 26/07/2022""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-ItemProperty-epo-cat-describesItem,
        cat-shape:epo-cat-ItemProperty-epo-cat-hasAttributeType,
        cat-shape:epo-cat-ItemProperty-epo-cat-hasClassificationScheme,
        cat-shape:epo-cat-ItemProperty-epo-cat-hasLiteralValue ;
    sh:targetClass :ItemProperty .

cat-shape:epo-cat-ItemRelation a sh:NodeShape ;
    rdfs:label "Item relation" ;
    rdfs:comment """Connection between two Items.

WG Approval: 30/06/2022
""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-ItemRelation-epo-cat-hasFactor,
        cat-shape:epo-cat-ItemRelation-epo-cat-isRelatedToItem ;
    sh:targetClass :ItemRelation .

cat-shape:epo-cat-ItemReplacement a sh:NodeShape ;
    rdfs:label "Item replacement" ;
    rdfs:comment """Supplantment, displacement or suspension of an Item by the described Item.

WG Approval: 30/06/2022
""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :ItemReplacement .

cat-shape:epo-cat-ItemRequirement a sh:NodeShape ;
    rdfs:label "Item requirement" ;
    rdfs:comment """Necessity of an Item by the described item. 

Additional Information:
The described item is provided without other items that are required for proper functionality.

For example, a printer cannot be used without ink cartridge; or a Server cannot be used without its installation. 

WG Approval: 19/05/2022

""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :ItemRequirement .

cat-shape:epo-cat-Line a sh:NodeShape ;
    rdfs:label "Line" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-Line-dct-description,
        cat-shape:epo-cat-Line-epo-cat-specifiesItem ;
    sh:targetClass :Line .

cat-shape:epo-cat-Manufacturer a sh:NodeShape ;
    rdfs:label "Manufacturer" ;
    rdfs:comment """The Producer of goods or provider of services. 

Additional Information:
Also, known as Supplier and Manufacturer.

We do not care who provides these service or products we care with whom Business is done.

PEPPOL - Invoice:
The Supplier is the Legal Person or Organisation who provides a product and/or service.

PEPPOL - Order: The supplier is the legal person or organisation who provides a product or service.
Examples of supplier roles: seller, despatch party, creditor, economic operator.

""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :Manufacturer .

cat-shape:epo-cat-PostAwardDocument a sh:NodeShape ;
    rdfs:label "Post award document" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-PostAwardDocument-epo-cat-hasDocumentType ;
    sh:targetClass :PostAwardDocument .

cat-shape:epo-cat-Price a sh:NodeShape ;
    rdfs:label "Price" ;
    rdfs:comment """Amount of money required or given in exchange for Items.

Additional information:
The Price excludes any charges (such as taxes, duties and fees) and discounts.

WG approval 26/07/2022""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:property cat-shape:epo-cat-Price-epo-cat-hasPriceType,
        cat-shape:epo-cat-Price-epo-hasPriceSurchargeInformation ;
    sh:targetClass :Price .

cat-shape:epo-cat-ProductSpecification a sh:NodeShape ;
    rdfs:label "Product specification" ;
    rdfs:comment """Document providing information about an Item. 

WG approval 07/04/2022

""" ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:targetClass :ProductSpecification .

cat-shape:epo-cat-Batch-epo-cat-hasBestBeforeDate a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype xsd:date ;
    sh:description """The day until which the quality of the item will remain optimal.

WG approval 10/03/2021""" ;
    sh:maxCount 1 ;
    sh:name "Has best before date" ;
    sh:path :hasBestBeforeDate .

cat-shape:epo-cat-Batch-epo-cat-hasExpiryDate a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype xsd:date ;
    sh:description """The last day until which the item can be used.

WG approval 10/03/2021
""" ;
    sh:maxCount 1 ;
    sh:name "Has expiry date" ;
    sh:path :hasExpiryDate .

cat-shape:epo-cat-Batch-epo-cat-hasManufactureDate a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype xsd:dateTime ;
    sh:description """The day of production.
""" ;
    sh:maxCount 1 ;
    sh:name "Has manufacture date" ;
    sh:path :hasManufactureDate .

cat-shape:epo-cat-Batch-epo-cat-isProductionOf a sh:PropertyShape ;
    sh:class :Item ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Is production of" ;
    sh:path :isProductionOf ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#isProductionOf> ?that . ?that <http://data.europa.eu/a4g/ontology#isProductionOf> ?this .} " ] .

cat-shape:epo-cat-Brand-dct-title a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description """A name given to the resource.

WG approval 30/05/2023
""" ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Title" ;
    sh:path dcterms:title .

cat-shape:epo-cat-Catalogue-epo-cat-comprisesCatalogueLine a sh:PropertyShape ;
    sh:class :CatalogueLine ;
    sh:minCount 1 ;
    sh:name "Comprises catalogue line" ;
    sh:path :comprisesCatalogueLine ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#comprisesCatalogueLine> ?that . ?that <http://data.europa.eu/a4g/ontology#comprisesCatalogueLine> ?this .} " ] .

cat-shape:epo-cat-Catalogue-epo-cat-hasActivityCode a sh:PropertyShape ;
    sh:class at-voc-new:activity-code ;
    sh:maxCount 1 ;
    sh:name "Has activity code" ;
    sh:path :hasActivityCode .

cat-shape:epo-cat-Catalogue-epo-cat-hasSpecificPaymentArrangement a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description """Information about payment conditions applied to all items in the catalogue.

Additional information:
This property may need to be used together with the payment arrangements provided in the ContractTerms, where such terms exists.

WG approval 30/06/2022

""" ;
    sh:maxCount 1 ;
    sh:name "Has specific payment arrangement" ;
    sh:path :hasSpecificPaymentArrangement .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasActivityCode a sh:PropertyShape ;
    sh:class at-voc-new:activity-code ;
    sh:maxCount 1 ;
    sh:name "Has activity code" ;
    sh:path :hasActivityCode .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasChargeInformation a sh:PropertyShape ;
    sh:class :ChargeInformation ;
    sh:name "Has charge information" ;
    sh:path :hasChargeInformation ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasChargeInformation> ?that . ?that <http://data.europa.eu/a4g/ontology#hasChargeInformation> ?this .} " ] .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasOrderableQuantityIncrement a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype xsd:integer ;
    sh:description "Orderable quantity increment: The increment of Orderable units that can be ordered." ;
    sh:maxCount 1 ;
    sh:name "Has orderable quantity increment" ;
    sh:path :hasOrderableQuantityIncrement .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasOrderableUnit a sh:PropertyShape ;
    sh:class at-voc-new:orderable-unit ;
    sh:description "The unit in which the item described in this catalogue line can be ordered." ;
    sh:maxCount 1 ;
    sh:name "Has orderable unit" ;
    sh:path :hasOrderableUnit .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasOrderableUnitFactorRate a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype xsd:decimal ;
    sh:description "The factor by which the Item net price of an unit of an item can be converted to the orderable unit." ;
    sh:maxCount 1 ;
    sh:name "Has orderable unit factor rate" ;
    sh:path :hasOrderableUnitFactorRate .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasPrice a sh:PropertyShape ;
    sh:class :Price ;
    sh:minCount 1 ;
    sh:name "Has price" ;
    sh:path :hasPrice ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasPrice> ?that . ?that <http://data.europa.eu/a4g/ontology#hasPrice> ?this .} " ] .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasStandardisedUnitPrice a sh:PropertyShape ;
    sh:class :Price ;
    sh:description """The item price based on a standardised unit.

Additional Information:
The unit must be indicated in the Quantity, with a quantity value of 1.""" ;
    sh:name "Has standardised unit price" ;
    sh:path :hasStandardisedUnitPrice ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasStandardisedUnitPrice> ?that . ?that <http://data.europa.eu/a4g/ontology#hasStandardisedUnitPrice> ?this .} " ] .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasTransactionConditions a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "Textual description of the specific transaction conditions (purchasing, sales, payment) for an item." ;
    sh:maxCount 1 ;
    sh:name "Has transaction conditions" ;
    sh:path :hasTransactionConditions .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasTransactionConditionsCode a sh:PropertyShape ;
    sh:class at-voc-new:transaction-conditions ;
    sh:description """Coded description of the specific transaction conditions (purchasing, sales, payment) for an item.
""" ;
    sh:maxCount 1 ;
    sh:name "Has transaction conditions code" ;
    sh:path :hasTransactionConditionsCode .

cat-shape:epo-cat-CatalogueLine-epo-cat-hasWarrantyInformation a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "Warranty information that applies to the catalogue line item." ;
    sh:maxCount 1 ;
    sh:name "Has warranty information" ;
    sh:path :hasWarrantyInformation .

cat-shape:epo-cat-CatalogueLine-epo-cat-isContractedItem a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype xsd:boolean ;
    sh:description "Indicator if the item is offered in accordance to an existing contract." ;
    sh:maxCount 1 ;
    sh:name "Is contracted item" ;
    sh:path :isContractedItem .

cat-shape:epo-cat-CatalogueLine-epo-cat-isOrderable a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype xsd:boolean ;
    sh:description "Indicator, if the item is orderable or not, e.g. because it is temporarily out of stock." ;
    sh:maxCount 1 ;
    sh:name "Is orderable" ;
    sh:path :isOrderable .

cat-shape:epo-cat-InformationHub-epo-cat-isSpecificToLine a sh:PropertyShape ;
    sh:class :Line ;
    sh:name "Is specific to line" ;
    sh:path :isSpecificToLine ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#isSpecificToLine> ?that . ?that <http://data.europa.eu/a4g/ontology#isSpecificToLine> ?this .} " ] .

cat-shape:epo-cat-Item-dct-description a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description """An account of the resource.

Additional Information:
Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource.

WG Approval 30/05/2023""" ;
    sh:maxCount 1 ;
    sh:name "Description" ;
    sh:path dcterms:description .

cat-shape:epo-cat-Item-dct-hasPart a sh:PropertyShape ;
    sh:class :Item ;
    sh:description """A related resource that is included either physically or logically in the described resource.

Additional Information: 
In the eCatalogue context, when this relation is used between two items, the resource in the definition refers to Items, i.e. one item is part of another item.

WG Approval: 30/06/2022
""" ;
    sh:name "Has part" ;
    sh:path dcterms:hasPart ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/hasPart> ?that . ?that <http://purl.org/dc/terms/hasPart> ?this .} " ] .

cat-shape:epo-cat-Item-dct-isReplacedBy a sh:PropertyShape ;
    sh:class :Item ;
    sh:description """A related resource that is supplanted, displaced, or superseded by the described resource.

Additional Information: 
In the eCatalogue context, when this relation is used between two items, the resource in the definition refers to Items, i.e. one item replaces another item. 

WG Approval: 30/06/2022
""" ;
    sh:name "Is replaced by" ;
    sh:path dcterms:isReplacedBy ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/isReplacedBy> ?that . ?that <http://purl.org/dc/terms/isReplacedBy> ?this .} " ] .

cat-shape:epo-cat-Item-dct-replaces a sh:PropertyShape ;
    sh:class :Item ;
    sh:description """A related resource that is supplanted, displaced, or superseded by the described resource.
""" ;
    sh:name "Replaces" ;
    sh:path dcterms:replaces ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/replaces> ?that . ?that <http://purl.org/dc/terms/replaces> ?this .} " ] .

cat-shape:epo-cat-Item-dct-requires a sh:PropertyShape ;
    sh:class :Item ;
    sh:description """A related resource that is required by the described resource to support its function, delivery, or coherence.

Additional Information:
In the eCatalogue context, when this relation is used between two items, the resource in the definition refers to Items, i.e. one Item requires another Item. 

WG Approval: 30/06/2022
""" ;
    sh:name "Requires" ;
    sh:path dcterms:requires ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/requires> ?that . ?that <http://purl.org/dc/terms/requires> ?this .} " ] .

cat-shape:epo-cat-Item-dct-title a sh:PropertyShape ;
    rdfs:isDefinedBy cat-shape:cat-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description """A name given to the resource.

WG approval 30/05/2023""" ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Title" ;
    sh:path dcterms:title .

cat-shape:epo-cat-Item-epo-cat-hasAccessoryItem a sh:PropertyShape ;
    sh:class :Item ;
    sh:description """An additional item that can enhance the original item. 

WG Approval: 19/05/2022
""" ;
    sh:name "Has accessory item" ;
    sh:path :hasAccessoryItem ;
    sh:sparql [ sh:select " SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasAccessoryItem> ?that . ?that <http://data.europa.eu/a4g/ontology#hasAccessoryItem> ?this .} " ] .

cat-shape:epo-cat-Item-epo-cat-hasBrand a sh:PropertyShape ;
    sh:class :Brand ;
    sh:maxCount 1 ;
    sh:name "Has brand" ;
