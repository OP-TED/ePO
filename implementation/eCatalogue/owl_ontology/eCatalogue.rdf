    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasAccessoryItem">
    <skos:prefLabel xml:lang="en">Has accessory item</skos:prefLabel>
    <skos:definition xml:lang="en">An item that can be added to another item to enhance it. WG Approval: 30/06/2022 (epo-cat:Item -&gt; epo-cat:Item (+epo-cat:hasAccessoryItem))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
