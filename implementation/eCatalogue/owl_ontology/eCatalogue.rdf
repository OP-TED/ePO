    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasExternalSpecification">
    <skos:prefLabel xml:lang="en">Has external specification</skos:prefLabel>
    <skos:definition xml:lang="en">URI reference to external item information or specifications, e.g. web address. (epo-cat:Item -&gt; epo:Document (+epo-cat:hasExternalSpecification))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
