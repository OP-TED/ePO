<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:a4g_shape="http://data.europa.eu/a4g/data-shape#"
    xmlns:at="http://publications.europa.eu/ontology/authority/"
    xmlns:sfrml="http://data.europa.eu/a4g/mapping/sf-rml/"
    xmlns:a4g="http://data.europa.eu/a4g/ontology#"
    xmlns:atold="http://publications.europa.eu/resource/authority/"
