  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasSerialID">
    <skos:prefLabel xml:lang="en">Has serial i d</skos:prefLabel>
    <skos:definition xml:lang="en">The identifier assigned to the specific instance of the produced concept. WG Approval 16/05/2023 (epo-cat:Item -&gt; adms:Identifier (+epo:hasSerialID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
