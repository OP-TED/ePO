    <skos:prefLabel xml:lang="en">Is orderable</skos:prefLabel>
    <skos:definition xml:lang="en">Indicator, if the item is orderable or not, e.g. because it is temporarily out of stock. (epo-cat:CatalogueLine)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#specifiesCatalogueReceiver">
