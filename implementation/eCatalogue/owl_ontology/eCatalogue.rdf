    xmlns:vann="http://purl.org/vocab/vann/"
    xmlns:org="http://www.w3.org/ns/org#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rml="http://semweb.mmlab.be/ns/rml#" >
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#isSpecificToCatalogueResponseLine">
    <skos:prefLabel xml:lang="en">Is specific to catalogue response line</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#isInStock">
    <skos:prefLabel xml:lang="en">Is in stock</skos:prefLabel>
    <skos:definition xml:lang="en">Indicator whether an item is in the supplier's stock. If not in stock the supplier has to order the item. (epo-cat:Item)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#Catalogue">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#PostAwardDocument"/>
    <skos:prefLabel xml:lang="en">Catalogue</skos:prefLabel>
    <skos:definition xml:lang="en">A Document describing a set of Items offered for purchase that can be processed in an electronic way.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasBatchID">
    <skos:prefLabel xml:lang="en">Has batch i d</skos:prefLabel>
    <skos:definition xml:lang="en">The identifier assigned to a specific batch of the produced Item. WG Approval 16/05/2023 (epo-cat:Batch -&gt; adms:Identifier (+epo:hasBatchID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasCertification">
    <skos:prefLabel xml:lang="en">Has certification</skos:prefLabel>
    <skos:definition xml:lang="en">Relation to the proof of conformance. WG approval 30/05/2023 (epo-cat:Item -&gt; epo:Certificate (+epo:hasCertification))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#Batch">
    <skos:prefLabel xml:lang="en">Batch</skos:prefLabel>
    <skos:definition xml:lang="en">A production Lot in which the Item instances were produced. WG Acceptance 10/03/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#isRelatedToItem">
    <skos:prefLabel xml:lang="en">Is related to item</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasNetMonetaryValue">
    <skos:prefLabel xml:lang="en">Has net monetary value</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#isSpecificToOrderLine">
    <skos:prefLabel xml:lang="en">Is specific to order line</skos:prefLabel>
    <skos:definition xml:lang="en">Information that is particular to one or multiple Order Lines. (epo-cat:ChargeInformation -&gt; epo-ord:OrderLine (+epo-ord:isSpecificToOrderLine))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasOrderableUnit">
    <skos:prefLabel xml:lang="en">Has orderable unit</skos:prefLabel>
    <skos:definition xml:lang="en">The unit in which the item described in this catalogue line can be ordered. (epo-cat:CatalogueLine -&gt; at-voc-new:orderable-unit (+epo-cat:hasOrderableUnit))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasFactor">
    <skos:prefLabel xml:lang="en">Has factor</skos:prefLabel>
    <skos:definition xml:lang="en">The number or the fraction of a related item. (epo-cat:ItemRelation)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#CatalogueResponse">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#PostAwardDocument"/>
    <skos:prefLabel xml:lang="en">Catalogue response</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasModel">
    <skos:prefLabel xml:lang="en">Has model</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#describesItem">
    <skos:prefLabel xml:lang="en">Describes item</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasBuyerItemID">
    <skos:prefLabel xml:lang="en">Has buyer item i d</skos:prefLabel>
    <skos:definition xml:lang="en">The general identifier assigned to the concept as defined by the Buyer.â€‹ WG approval 16/05/2023 (epo-cat:Item -&gt; adms:Identifier (+epo:hasBuyerItemID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#isProductionOf">
    <skos:prefLabel xml:lang="en">Is production of</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasChargeInformation">
    <skos:prefLabel xml:lang="en">Has charge information</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasDeliveryClassification">
    <skos:prefLabel xml:lang="en">Has delivery classification</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasDimensionDescription">
    <skos:prefLabel xml:lang="en">Has dimension description</skos:prefLabel>
    <skos:definition xml:lang="en">Textual description of the item's dimensions. (epo-cat:Item)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ItemComposition">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ItemRelation"/>
    <skos:prefLabel xml:lang="en">Item composition</skos:prefLabel>
    <skos:definition xml:lang="en">Inclusion of an Item into in the described Item. Additional Information: The Item is provided with its components, however, the component Items may also be provided separately. For example, a bicycle has wheels as components, but they may be provided as spare parts as well. This relation is useful to describe different package levels of an item or hierarchies of Items. WG Approval: 19/05/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasQualifiedItemRelation">
    <skos:prefLabel xml:lang="en">Has qualified item relation</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasSpecification">
    <skos:prefLabel xml:lang="en">Has specification</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ItemAccessoryRelation">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ItemRelation"/>
    <skos:prefLabel xml:lang="en">Item accessory relation</skos:prefLabel>
    <skos:definition xml:lang="en">Combination of two Items where the former enhances the latter. WG Approval: 30/06/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasKeyword">
    <skos:prefLabel xml:lang="en">Has keyword</skos:prefLabel>
    <skos:definition xml:lang="en">List of words (e.g, synonyms) to make the item searchable. WG Approval 7/04/2022 (epo-cat:Item)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#specifiesBuyer">
    <skos:prefLabel xml:lang="en">Specifies buyer</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasDocumentStatus">
    <skos:prefLabel xml:lang="en">Has document status</skos:prefLabel>
    <skos:definition xml:lang="en">https://test-docs.peppol.eu/logistics/transport-execution/codelist/DocumentStatusCode/ (epo-cat:PostAwardDocument -&gt; at-voc-new:document-status (+epo:hasDocumentStatus))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasItemStandardID">
    <skos:prefLabel xml:lang="en">Has item standard i d</skos:prefLabel>
    <skos:definition xml:lang="en">The general identifier assigned to the concept based on a standard scheme.â€‹ Additional information: For example the GTIN scheme (Global Trade Item Number). WG approval 16/05/2023 (epo-cat:Item -&gt; adms:Identifier (+epo:hasItemStandardID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasCatalogueLineValidity">
    <skos:prefLabel xml:lang="en">Has catalogue line validity</skos:prefLabel>
    <skos:definition xml:lang="en">The relation indicating until when a Catalogue Line instance is applicable. WG approval 21/09/2023 (epo-cat:CatalogueLine -&gt; epo:Period (+epo-cat:hasCatalogueLineValidity))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasBestBeforeDate">
    <skos:prefLabel xml:lang="en">Has best before date</skos:prefLabel>
    <skos:definition xml:lang="en">The day until which the quality of the item will remain optimal. WG approval 10/03/2021 (epo-cat:Batch)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasOrderabableUnitFactorRate">
    <skos:prefLabel xml:lang="en">Has orderabable unit factor rate</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasItemClassification">
    <skos:prefLabel xml:lang="en">Has item classification</skos:prefLabel>
    <skos:definition xml:lang="en">A classification of this item according to a specific system for classifying products or services. Additional information: Examples of classification schemes are: CPV, UNSPC, eCl@ss, GPC, ... WG acceptance 10/03/2022 (epo-cat:Item -&gt; at-voc-new:item-classification (+epo-cat:hasItemClassification))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasDocumentType">
    <skos:prefLabel xml:lang="en">Has document type</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#Line">
    <skos:prefLabel xml:lang="en">Line</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasValidityPeriod">
    <skos:prefLabel xml:lang="en">Has validity period</skos:prefLabel>
    <skos:definition xml:lang="en">The relation indicating until when a given instance of a concept is applicable. WG approval 30/05/2023 (epo-cat:Catalogue -&gt; epo:Period (+epo:hasValidityPeriod))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#specifiesCatalogueProvider">
    <skos:prefLabel xml:lang="en">Specifies catalogue provider</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#specifiesItem">
    <skos:prefLabel xml:lang="en">Specifies item</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasOrderableUnitFactorRate">
    <skos:prefLabel xml:lang="en">Has orderable unit factor rate</skos:prefLabel>
    <skos:definition xml:lang="en">The factor by which the Item net price of an unit of an item can be converted to the orderable unit. (epo-cat:CatalogueLine)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ItemModel">
    <skos:prefLabel xml:lang="en">Item model</skos:prefLabel>
    <skos:definition xml:lang="en">A specific design or version of a product or service. Additional information: This class is meant as a reusable reference to the model name rather than as a set of specifications of an Item of this particular model. WG acceptance 10/03/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ItemRelation">
    <skos:prefLabel xml:lang="en">Item relation</skos:prefLabel>
    <skos:definition xml:lang="en">Connection between two Items. WG Approval: 30/06/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasDeliveryLocation">
    <skos:prefLabel xml:lang="en">Has delivery location</skos:prefLabel>
    <skos:definition xml:lang="en">Location delivery area where the Item can be ordered to the given price. (epo-cat:Price -&gt; dct:Location (+epo-cat:hasDeliveryLocation))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#Brand">
    <skos:prefLabel xml:lang="en">Brand</skos:prefLabel>
    <skos:definition xml:lang="en">Name given by an organisation to its product or service, or range of products or services. Additional information: Brand name allows economic operators to distinctively describe an item in their eCatalogue. This may be different from the "item name". WG acceptance 10/03/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasPrice">
    <skos:prefLabel xml:lang="en">Has price</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasManufacturerID">
    <skos:prefLabel xml:lang="en">Has manufacturer i d</skos:prefLabel>
    <skos:definition xml:lang="en">The manufacturer's identifier for the item. (epo-cat:Manufacturer -&gt; adms:Identifier (+epo:hasManufacturerID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasOrderableQuantityIncrement">
    <skos:prefLabel xml:lang="en">Has orderable quantity increment</skos:prefLabel>
    <skos:definition xml:lang="en">Orderable quantity increment: The increment of Orderable units that can be ordered. (epo-cat:CatalogueLine)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#Manufacturer">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#OfferingParty"/>
    <skos:prefLabel xml:lang="en">Manufacturer</skos:prefLabel>
    <skos:definition xml:lang="en">The Producer of goods or provider of services. Additional Information: Also, known as Supplier and Manufacturer. We do not care who provides these service or products we care with whom Business is done. PEPPOL - Invoice: The Supplier is the Legal Person or Organisation who provides a product and/or service. PEPPOL - Order: The supplier is the legal person or organisation who provides a product or service. Examples of supplier roles: seller, despatch party, creditor, economic operator.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#refersToCatalogue">
    <skos:prefLabel xml:lang="en">Refers to catalogue</skos:prefLabel>
    <skos:definition xml:lang="en">Additional information: If an order refers to multiple catalogues then all these catalogues ideally are subordinated to the same contract. Some countries however may allow multiple contracts per order (i.e Italy). (epo-ord:Order -&gt; epo-cat:Catalogue (+epo-ord:refersToCatalogue))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#specifiesSeller">
    <skos:prefLabel xml:lang="en">Specifies seller</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasManufacturer">
    <skos:prefLabel xml:lang="en">Has manufacturer</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasMaximumOrderQuantity">
    <skos:prefLabel xml:lang="en">Has maximum order quantity</skos:prefLabel>
    <skos:definition xml:lang="en">The maximum number of orderable units that can be ordered according to details provided in the catalogue line, such as price. (epo-cat:CatalogueLine -&gt; epo:Quantity (+epo-cat:hasMaximumOrderQuantity))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#comprisesCatalogueLine">
    <skos:prefLabel xml:lang="en">Comprises catalogue line</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasBaseQuantity">
    <skos:prefLabel xml:lang="en">Has base quantity</skos:prefLabel>
    <skos:definition xml:lang="en">The quantity at which the net monetary value applies. (epo-cat:Price -&gt; epo:Quantity (+epo-cat:hasBaseQuantity))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasAttributeType">
    <skos:prefLabel xml:lang="en">Has attribute type</skos:prefLabel>
    <skos:definition xml:lang="en">The property defined in a classification scheme. Additional Information: For example, the following two properties are defined in the ECLASS classification of goods and services: (0173-1#02-AAA026#007) drilling diameter: drilling diameter, (0173-1#02-AAA030#007) flange diameter: flange diameter . WG approval 28/07/2022 (epo-cat:ItemProperty)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#CatalogueResponseLine">
    <skos:prefLabel xml:lang="en">Catalogue response line</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasTaxScheme">
    <skos:prefLabel xml:lang="en">Has tax scheme</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasHazardousItemUNDGCode">
    <skos:prefLabel xml:lang="en">Has hazardous item u n d g code</skos:prefLabel>
    <skos:definition xml:lang="en">Specification of the hazardous nature of an item according to the UNDG classification schema. Provides detail of the classification and nature of a hazardous item. (epo-cat:Item -&gt; at-voc-new:dangerous-goods-list (+epo-cat:hasHazardousItemUNDGCode))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasTransactionConditions">
    <skos:prefLabel xml:lang="en">Has transaction conditions</skos:prefLabel>
    <skos:definition xml:lang="en">Textual description of the specific transaction conditions (purchasing, sales, payment) for an item. (epo-cat:CatalogueLine)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ChargeInformation">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#AllowanceChargeInformation"/>
    <skos:prefLabel xml:lang="en">Charge information</skos:prefLabel>
    <skos:definition xml:lang="en">Information about tax, fee or duty imposed. Additional Information: Charge category indicates the nature of the tax/duty/fee, for example VAT, CAR, etc. Charge category modifier may be used in case different levels, exemptions or other modifications apply. The charge can be fixed or relative to the price. WG approval 16/05/2023</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasManufactureDate">
    <skos:prefLabel xml:lang="en">Has manufacture date</skos:prefLabel>
    <skos:definition xml:lang="en">The day of production. (epo-cat:Batch)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasUUID">
    <skos:prefLabel xml:lang="en">Has u u i d</skos:prefLabel>
    <skos:definition xml:lang="en">A universally unique identifier for an instance of this document. WG Approval 12/05/2020 (epo-cat:CatalogueResponseLine -&gt; adms:Identifier (+epo:hasUUID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasTaxInformation">
    <skos:prefLabel xml:lang="en">Has tax information</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasExpectedDeliveryTime">
    <skos:prefLabel xml:lang="en">Has expected delivery time</skos:prefLabel>
    <skos:definition xml:lang="en">The expected amount of time between the order and delivery of an item. WG approval 26/07/2022 (epo-cat:Price -&gt; epo:Duration (+epo-cat:hasExpectedDeliveryTime))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasSpecificPaymentArrangement">
    <skos:prefLabel xml:lang="en">Has specific payment arrangement</skos:prefLabel>
    <skos:definition xml:lang="en">Information about payment conditions applied to all items in the catalogue. Additional information: This property may need to be used together with the payment arrangements provided in the ContractTerms, where such terms exists. WG approval 30/06/2022 (epo-cat:Catalogue)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasCountryOfOrigin">
    <skos:prefLabel xml:lang="en">Has country of origin</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#InformationHub">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ContextDescription"/>
    <skos:prefLabel xml:lang="en">Information hub</skos:prefLabel>
    <skos:definition xml:lang="en">Grouping of data that may be associated to various concepts. Additional Information: For example, AllowanceChargeInformation may be associated to the Order or the Catalogue (either at the Line level or at the Document level), amongst others. WG approval 16/05/2023</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasAcceptanceStatus">
    <skos:prefLabel xml:lang="en">Has acceptance status</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#isContractedItem">
    <skos:prefLabel xml:lang="en">Is contracted item</skos:prefLabel>
    <skos:definition xml:lang="en">Indicator if the item is offered in accordance to an existing contract. (epo-cat:CatalogueLine)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasLiteralValue">
    <skos:prefLabel xml:lang="en">Has literal value</skos:prefLabel>
    <skos:definition xml:lang="en">Simple value of the property, which is defined in a classification scheme. Additional Information: For example, the (0173-1#02-AAV139#001) coating 1 of an optical material has value of "silver". This simple literal value is for the cases when the property value does not have units, in which case the qualifiedValue shall be used. WG approval 28/07/2022 (epo-cat:ItemProperty)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasExpiryDate">
    <skos:prefLabel xml:lang="en">Has expiry date</skos:prefLabel>
    <skos:definition xml:lang="en">The last day until which the item can be used. WG approval 10/03/2021 (epo-cat:Batch)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#CatalogueLine">
    <skos:prefLabel xml:lang="en">Catalogue line</skos:prefLabel>
    <skos:definition xml:lang="en">Aggregated information about the Terms and specifications for procuring an Item. Details about an Item, like Prices, packaging, discounts, special promotions, etc. TC 440: Detailed information of an Item, Price and its composition in a Catalogue. Composition: e.g. information referring to its description, how to order it, what it is made of, properties, delivery conditions, etc.</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#specifiesChargeInformation">
    <skos:prefLabel xml:lang="en">Specifies charge information</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasWarrantyInformation">
    <skos:prefLabel xml:lang="en">Has warranty information</skos:prefLabel>
    <skos:definition xml:lang="en">Warranty information that applies to the catalogue line item. (epo-cat:CatalogueLine)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasMinimumQuantityGuaranteedForDelivery">
    <skos:prefLabel xml:lang="en">Has minimum quantity guaranteed for delivery</skos:prefLabel>
    <skos:definition xml:lang="en">The minimum quantity of an item that is guaranteed by the seller to be delivered. (epo-cat:CatalogueLine -&gt; epo:Quantity (+epo-cat:hasMinimumQuantityGuaranteedForDelivery))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#CatalogueResponseInformation">
    <skos:prefLabel xml:lang="en">Catalogue response information</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasPriceType">
    <skos:prefLabel xml:lang="en">Has price type</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ItemProperty">
    <skos:prefLabel xml:lang="en">Item property</skos:prefLabel>
    <skos:definition xml:lang="en">Construct meant to represent the association between an attribute defined in an external classification scheme and the value ascribed to it. WG approval 26/07/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasNetQuantity">
    <skos:prefLabel xml:lang="en">Has net quantity</skos:prefLabel>
    <skos:definition xml:lang="en">The net quantity of the item that is contained in each consumable unit, excluding any packaging materials. (epo-cat:Item -&gt; epo:Quantity (+epo-cat:hasNetQuantity))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasSellerItemID">
    <skos:prefLabel xml:lang="en">Has seller item i d</skos:prefLabel>
    <skos:definition xml:lang="en">The general identifier assigned to the concept as defined by the Seller.â€‹ WG approval 16/05/2023 (epo-cat:Item -&gt; adms:Identifier (+epo:hasSellerItemID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasPriceDiscountInformation">
    <skos:prefLabel xml:lang="en">Has price discount information</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#comprisesCatalogueResponseLine">
    <skos:prefLabel xml:lang="en">Comprises catalogue response line</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasClassificationScheme">
    <skos:prefLabel xml:lang="en">Has classification scheme</skos:prefLabel>
    <skos:definition xml:lang="en">The classification scheme where a property is defined. Additional Information: For example, the following two properties are defined in the ECLASS classification of goods and services: (0173-1#02-AAA026#007) drilling diameter: drilling diameter, (0173-1#02-AAA030#007) flange diameter: flange diameter . WG approval 28/07/2022 (epo-cat:ItemProperty)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasActivityCode">
    <skos:prefLabel xml:lang="en">Has activity code</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasPriceValidity">
    <skos:prefLabel xml:lang="en">Has price validity</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ItemComplementaryRelation">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ItemRelation"/>
    <skos:prefLabel xml:lang="en">Item complementary relation</skos:prefLabel>
    <skos:definition xml:lang="en">Combination of two Items which mutually enhance one another. WG Approval: 30/06/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasTransactionConditionsCode">
    <skos:prefLabel xml:lang="en">Has transaction conditions code</skos:prefLabel>
    <skos:definition xml:lang="en">Coded description of the specific transaction conditions (purchasing, sales, payment) for an item. (epo-cat:CatalogueLine -&gt; at-voc-new:transaction-conditions (+epo-cat:hasTransactionConditionsCode))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#refersToCatalogueLine">
    <skos:prefLabel xml:lang="en">Refers to catalogue line</skos:prefLabel>
    <skos:definition xml:lang="en">Additional Information: The reference to the catalogue line is for information only, to trace the source of the information provided in the order line. (epo-ord:OrderLine -&gt; epo-cat:CatalogueLine (+epo-ord:refersToCatalogueLine))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasTaxCategory">
    <skos:prefLabel xml:lang="en">Has tax category</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasStandardisedUnitPrice">
    <skos:prefLabel xml:lang="en">Has standardised unit price</skos:prefLabel>
    <skos:definition xml:lang="en">The item price based on a standardised unit. Additional Information: The unit must be indicated in the Quantity, with a quantity value of 1. (epo-cat:CatalogueLine -&gt; epo-cat:Price (+epo-cat:hasStandardisedUnitPrice))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasComplementaryItem">
    <skos:prefLabel xml:lang="en">Has complementary item</skos:prefLabel>
    <skos:definition xml:lang="en">An item that if used with another item provides mutual enhancements to both of them. Additional information: For example, gin and tonic; or smart-phone and smart-watch. WG Approval: 19/05/2022 (epo-cat:Item -&gt; epo-cat:Item (+epo-cat:hasComplementaryItem))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#ItemRequirement">
    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#ItemRelation"/>
    <skos:prefLabel xml:lang="en">Item requirement</skos:prefLabel>
    <skos:definition xml:lang="en">Necessity of an Item by the described item. Additional Information: The described item is provided without other items that are required for proper functionality. For example, a printer cannot be used without ink cartridge; or a Server cannot be used without its installation. WG Approval: 19/05/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#Price">
    <skos:prefLabel xml:lang="en">Price</skos:prefLabel>
    <skos:definition xml:lang="en">Amount of money required or given in exchange for Items. Additional information: The Price excludes any charges (such as taxes, duties and fees) and discounts. WG approval 26/07/2022</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasQuantityThreshold">
    <skos:prefLabel xml:lang="en">Has quantity threshold</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasHazardousClass">
    <skos:prefLabel xml:lang="en">Has hazardous class</skos:prefLabel>
    <skos:definition xml:lang="en">Specification of the hazardous nature of an item according to a classification schema. Provides detail of the classification and nature of a hazardous item. (epo-cat:Item)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
