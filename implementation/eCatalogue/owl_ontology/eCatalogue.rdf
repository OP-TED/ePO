    <skos:prefLabel xml:lang="en">Has manufacturer item i d</skos:prefLabel>
    <skos:definition xml:lang="en">The general identifier assigned to the concept as defined by the Manufacturer. â€‹ WG approval 16/05/2023 (epo-cat:Item -&gt; adms:Identifier (+epo:hasManufacturerItemID))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#cat"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#foreseesPackage">
