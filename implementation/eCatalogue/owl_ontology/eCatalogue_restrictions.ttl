epo:Brand a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:hasName ] .

epo:CatalogueDocument a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:instantiatesCatalogue ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:hasName ] .

epo:ItemModel a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
