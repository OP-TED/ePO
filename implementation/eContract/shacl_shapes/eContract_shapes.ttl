@prefix schema: <http://schema.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix cpov: <http://data.europa.eu/m8g/> .
@prefix a4g: <http://data.europa.eu/a4g/ontology#> .
@prefix sfrml: <http://data.europa.eu/a4g/mapping/sf-rml/> .
@prefix a4g_shape: <http://data.europa.eu/a4g/data-shape#> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix m8g: <http://data.europa.eu/m8g/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix person: <http://www.w3.org/ns/person#> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix shacl: <http://www.w3.org/ns/shacl#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix r2rml: <http://www.w3.org/ns/r2rml#> .
@prefix at: <http://publications.europa.eu/ontology/authority/> .
@prefix atold: <http://publications.europa.eu/resource/authority/> .
@prefix vann: <http://purl.org/vocab/vann/> .

a4g_shape:PlainLiteral a shacl:NodeShape ;
	shacl:or (
		[
			shacl:datatype xsd:string ;
		]
		[
			shacl:datatype rdf:langString ;
		]
	) .

a4g_shape:ContractModificationInformationShape a shacl:NodeShape ;
	shacl:targetClass a4g:ContractModificationInformation ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:property a4g_shape:ContractModificationInformation-hasModificationDescription-PlainLiteral, a4g_shape:ContractModificationInformation-hasModificationReasonDescription-PlainLiteral, a4g_shape:ContractModificationInformation-relatesToEFormSectionIdentifier-Identifier, a4g_shape:ContractModificationInformation-concernsContractAmendment-ContractAmendment, a4g_shape:ContractModificationInformation-hasModificationJustification-modification-justification .

a4g_shape:con-shape a owl:Ontology ;
	rdfs:label "eProcurement Ontology con - SHACL shapes"@en ;
	dcterms:created "2021-06-01"^^xsd:date ;
	dcterms:description "The eProcurement Ontology con SHACL shapes provides the datashape specifications for the eProcurement Ontology con module."@en ;
	dcterms:issued "2024-03-21"^^xsd:date ;
	dcterms:license "Â© European Union, 2014. Unless otherwise noted, the reuse of the Ontology is authorised under the European Union Public Licence v1.2 (https://eupl.eu/)." ;
	dcterms:publisher "http://publications.europa.eu/resource/authority/corporate-body/PUBL" ;
	dcterms:title "eProcurement Ontology con - SHACL shapes"@en ;
	vann:preferredNamespacePrefix "epo" ;
	vann:preferredNamespaceUri "http://data.europa.eu/a4g/ontology#" ;
	rdfs:seeAlso <https://docs.ted.europa.eu/home/index.html>, <https://github.com/OP-TED/ePO/releases>, <https://joinup.ec.europa.eu/collection/eprocurement/solution/eprocurement-ontology/about>, <https://op.europa.eu/en/web/eu-vocabularies/e-procurement> ;
	owl:imports <http://data.europa.eu/eli/ontology#>, <http://data.europa.eu/m8g/>, <http://purl.org/dc/terms/>, <http://purl.org/vocab/vann/>, <http://www.w3.org/2004/02/skos/core>, <http://www.w3.org/2006/time>, <http://www.w3.org/ns/adms>, <http://www.w3.org/ns/locn>, <http://www.w3.org/ns/org#>, <http://www.w3.org/ns/person>, <http://xmlns.com/foaf/0.1/> ;
	owl:incompatibleWith "3.1.0" ;
	owl:priorVersion "http://data.europa.eu/a4g/data-shape#con-shape-4.0.2" ;
	owl:versionIRI a4g_shape:con-shape-4.1.0 ;
	owl:versionInfo "4.1.0" .

a4g_shape:ContractModificationInformation-hasModificationDescription-PlainLiteral a shacl:PropertyShape ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:hasModificationDescription ;
	shacl:description "An explanatory text about this context. Additional Information This corresponds in Standard forms to Field VII.2.1 in F20 This corresponds in eForms to BT-202 . WG Approval 5/11/2019 " ;
	shacl:name "has modification description"@en ;
	shacl:minCount 0 ;
	shacl:maxCount 1 ;
	shacl:node a4g_shape:PlainLiteral .

a4g_shape:ContractModificationInformation-hasModificationReasonDescription-PlainLiteral a shacl:PropertyShape ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:hasModificationReasonDescription ;
	shacl:description "An explanation about the reasons for using the concept. Additional Information This corresponds in Standard forms to Field VII.2.2.2 and VII.2.2.4 in F20. This corresponds in eForms to BT-201 . WG Approval 5/11/2019 " ;
	shacl:name "has modification reason description"@en ;
	shacl:minCount 0 ;
	shacl:maxCount 1 ;
	shacl:node a4g_shape:PlainLiteral .

a4g_shape:ContractAmendmentShape a shacl:NodeShape ;
	shacl:targetClass a4g:ContractAmendment ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:property a4g_shape:ContractAmendment-updatesContractValue-MonetaryValue, a4g_shape:ContractAmendment-amendsContract-Contract ;
	rdfs:comment "A change to an existing Contract that is agreed by the contracting parties." .

a4g_shape:ContractAmendment-updatesContractValue-MonetaryValue a shacl:PropertyShape ;
	shacl:targetClass a4g:MonetaryValue ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:updatesContractValue ;
	shacl:name "updates contract value"@en ;
	shacl:maxCount 1 ;
	shacl:nodeKind shacl:IRI .

a4g_shape:DeliverableShape a shacl:NodeShape ;
	shacl:targetClass a4g:Deliverable ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:property a4g_shape:Deliverable-hasQuantity-Quantity, a4g_shape:Deliverable-hasTotalValue-MonetaryValue .

a4g_shape:Deliverable-hasQuantity-Quantity a shacl:PropertyShape ;
	shacl:targetClass a4g:Quantity ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:hasQuantity ;
	shacl:name "has quantity"@en ;
	shacl:maxCount 1 ;
	shacl:nodeKind shacl:IRI .

a4g_shape:ContractAmendment-amendsContract-Contract a shacl:PropertyShape ;
	shacl:targetClass a4g:Contract ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:amendsContract ;
	shacl:name "amends contract"@en ;
	shacl:minCount 1 ;
	shacl:maxCount 1 ;
	shacl:nodeKind shacl:IRI .

a4g_shape:ContractModificationInformation-relatesToEFormSectionIdentifier-Identifier a shacl:PropertyShape ;
	shacl:targetClass adms:Identifier ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:relatesToEFormSectionIdentifier ;
	shacl:name "relates to e form section identifier"@en ;
	shacl:maxCount 1 ;
	shacl:nodeKind shacl:IRI .

a4g_shape:ContractModificationInformation-concernsContractAmendment-ContractAmendment a shacl:PropertyShape ;
	shacl:targetClass a4g:ContractAmendment ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:concernsContractAmendment ;
	shacl:name "concerns contract amendment"@en ;
	shacl:minCount 1 ;
	shacl:maxCount 1 ;
	shacl:nodeKind shacl:IRI .

a4g_shape:ContractModificationInformation-hasModificationJustification-modification-justification a shacl:PropertyShape ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:hasModificationJustification ;
	shacl:description "Explanation of why a contract was modified. WG Approval 09/11/2021 The codelist to be used is at-voc:modification-justification which is available at http://publications.europa.eu/resource/dataset/modification-justification Additional Information This corresponds in Standard forms to Field VII.2.2.1 and VII.2.2.3 in F20 . This corresponds in eForms to BT-200. " ;
	shacl:name "has modification justification"@en ;
	shacl:minCount 1 ;
	shacl:maxCount 1 ;
	shacl:node [
		rdf:type shacl:NodeShape ;
		shacl:property [
			shacl:path skos:inScheme ;
			shacl:hasValue atold:modification-justification ;
		] ;
	] ;
	shacl:nodeKind shacl:IRI .

a4g_shape:Deliverable-hasTotalValue-MonetaryValue a shacl:PropertyShape ;
	shacl:targetClass a4g:MonetaryValue ;
	rdfs:isDefinedBy a4g_shape:con-shape ;
	shacl:path a4g:hasTotalValue ;
	shacl:name "has total value"@en ;
	shacl:maxCount 1 ;
	shacl:nodeKind shacl:IRI .

