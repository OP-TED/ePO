epo:agreedBySeller rdfs:domain epo:ShipmentAgreement ;
    rdfs:range epo:Seller .

epo:comprisesOrderResponseLine rdfs:domain epo:OrderResponse ;
    rdfs:range epo:OrderResponseLine .

epo:concernsContract rdfs:domain epo:ContractInformation ;
    rdfs:range epo:Contract .

epo:concernsOriginatorRequest rdfs:domain epo:OriginatorInformation ;
    rdfs:range epo:OriginatorRequest .

epo:hasAcceptanceStatus rdfs:domain epo:OrderResponseInformation ;
    rdfs:range <skos:Concept> .

epo:hasAllowanceChargeReason rdfs:domain epo:AllowanceChargeInformation ;
    rdfs:range <skos:Concept> .

epo:hasAllowanceChargeReasonDescription rdfs:domain epo:AllowanceChargeInformation ;
    rdfs:range rdf:PlainLiteral .

epo:hasAmount rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AllowanceChargeInformation epo:TaxInformation ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:MonetaryValue ) ] .

epo:hasAmountDueForPayment rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasDeliveryPeriod rdfs:domain epo:DeliveryInformation ;
    rdfs:range epo:Period .

epo:hasDocumentType rdfs:domain epo:Document ;
    rdfs:range <skos:Concept> .

epo:hasPrepaidAmount rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasPrice rdfs:domain epo:OrderLine ;
    rdfs:range epo:Price .

epo:hasPriceDiscountInformation rdfs:domain epo:Price ;
    rdfs:range epo:AllowanceInformation .

epo:hasQuantity rdfs:domain epo:OrderLine ;
    rdfs:range epo:Quantity .

epo:hasRoundingAmount rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasShippingMark rdfs:domain epo:DeliveryInformation ;
    rdfs:range rdf:PlainLiteral .

epo:hasTaxCategory rdfs:domain epo:TaxInformation ;
    rdfs:range <skos:Concept> .

epo:hasTaxScheme rdfs:domain epo:TaxInformation ;
    rdfs:range <skos:Concept> .

epo:hasTotalAllowanceAmount rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasTotalChargeAmount rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasTotalLineAmount rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasTotalTaxExclusiveAmount rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasTotalTaxInclusiveAmount rdfs:domain epo:Order ;
    rdfs:range epo:MonetaryValue .

epo:hasTrackingID rdfs:domain epo:DeliveryInformation ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasVATCategory rdfs:domain epo:TaxInformation ;
    rdfs:range <skos:Concept> .

epo:implementsContract rdfs:domain epo:OrderResponse ;
    rdfs:range epo:Contract .

epo:isCalculatedOn rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AllowanceChargeInformation epo:TaxInformation ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:MonetaryValue ) ] .

epo:isSpecificToOrderLine rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AllowanceInformation epo:ContractInformation epo:DeliveryInformation epo:ChargeInformation epo:OriginatorInformation ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:OrderLine ) ] .

epo:isSpecificToOrderResponseLine rdfs:domain epo:OrderResponseInformation ;
    rdfs:range epo:OrderResponseLine .

epo:refersToCatalogue rdfs:domain epo:Order ;
    rdfs:range epo:Catalogue .

epo:refersToCatalogueLine rdfs:domain epo:OrderLine ;
    rdfs:range epo:CatalogueLine .

epo:refersToDeliveryInformation rdfs:domain epo:ShipmentInformation ;
    rdfs:range epo:DeliveryInformation .

epo:refersToOrder rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:Order .

epo:refersToOrderLine rdfs:domain epo:DespatchLine ;
    rdfs:range epo:OrderLine .

epo:refersToProject rdfs:domain epo:Order ;
    rdfs:range epo:Project .

epo:specifiesBuyer rdfs:domain epo:Order,
        epo:OrderResponse ;
    rdfs:range epo:Buyer .

epo:specifiesChargeInformation rdfs:domain epo:Order ;
    rdfs:range epo:ChargeInformation .

epo:specifiesConsignee rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:Consignee .

epo:specifiesContractInformation rdfs:domain epo:Order ;
    rdfs:range epo:ContractInformation .

epo:specifiesDeliveryAgreementLocation rdfs:domain epo:DeliveryAgreement ;
    rdfs:range dcterms:Location .

epo:specifiesDespatcher rdfs:domain epo:Order ;
    rdfs:range epo:Despatcher .

epo:specifiesDiscountInformation rdfs:domain epo:Order ;
    rdfs:range epo:AllowanceInformation .

epo:specifiesOrderDelivery rdfs:domain epo:SimpleOrder ;
    rdfs:range epo:DeliveryInformation .

epo:specifiesOrderLineDelivery rdfs:domain epo:AdvancedOrderLine ;
    rdfs:range epo:DeliveryInformation .

epo:specifiesOrderResponseInformation rdfs:domain epo:OrderResponse ;
    rdfs:range epo:OrderResponseInformation .

epo:specifiesOriginator rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:Originator .

epo:specifiesPlaceOfDelivery rdfs:domain epo:DeliveryInformation ;
    rdfs:range dcterms:Location .

<http://data.europa.eu/a4g/rule> a owl:Ontology ;
    rdfs:label """eProcurement Ontology - eOrdering. This module provides the inference-related definitions.
            """@en ;
    cc:attributionName "PublicationsOffice of the European Union" ;
    cc:attributionURL <http://publications.europa.eu/resource/authority/corporate-body/PUBL> ;
