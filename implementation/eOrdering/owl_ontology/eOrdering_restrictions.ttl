dcterms:description rdfs:domain epo:DeliveryAgreement ;
    rdfs:range rdf:PlainLiteral .

epo:comprisesOrderLine rdfs:domain epo:Order ;
    rdfs:range epo:OrderLine .

epo:concernsConsignee rdfs:domain epo:DeliveryInformation ;
    rdfs:range epo:Consignee .

epo:concernsOriginator a owl:FunctionalProperty ;
    rdfs:domain epo:OriginatorInformation ;
    rdfs:range epo:Originator .

epo:hasAccountingCost rdfs:domain epo:Order ;
    rdfs:range rdf:PlainLiteral .

epo:hasCustomerReference rdfs:domain epo:Order ;
    rdfs:range xsd:boolean .

epo:hasDeliveryDeadline rdfs:domain epo:DeliveryInformation ;
    rdfs:range xsd:dateTime .

epo:hasPaymentTerm rdfs:domain epo:Order ;
    rdfs:range rdf:PlainLiteral .

epo:hasResponseDescription rdfs:domain epo:OrderResponse ;
    rdfs:range rdf:PlainLiteral .

epo:hasTaxInformation rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AllowanceChargeInformation epo:Item ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:TaxInformation ) ] .

epo:isSubmittedForOrder a owl:FunctionalProperty ;
    rdfs:domain epo:OrderResponse ;
    rdfs:range epo:Order .

epo:isSubmittedForOrderLine rdfs:domain epo:OrderResponseLine ;
    rdfs:range epo:OrderLine .

epo:specifiesDeliveryInformation rdfs:domain epo:Order ;
    rdfs:range epo:DeliveryInformation .

epo:specifiesGeneralDeliveryAgreement rdfs:domain epo:DeliveryInformation ;
    rdfs:range epo:DeliveryAgreement .

epo:specifiesOriginatorInformation rdfs:domain epo:Order ;
    rdfs:range epo:OriginatorInformation .

epo:specifiesSpecificDeliveryAgreement rdfs:domain epo:DeliveryInformation ;
    rdfs:range epo:DeliveryAgreement .

epo:hasPercentage rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AllowanceChargeInformation epo:TaxInformation ) ] ;
    rdfs:range xsd:decimal .

epo:specifiesSeller a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:Order epo:Catalogue ) ],
        epo:OrderResponse ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:Seller ) ],
        epo:Seller .

epo:OrderResponseLine a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:isSubmittedForOrderLine ] .

epo:OriginatorInformation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:concernsOriginator ] .

epo:AllowanceChargeInformation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasTaxInformation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasPercentage ] .

epo:OrderResponse a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:isSubmittedForOrder ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:specifiesSeller ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasResponseDescription ] .

epo:TaxInformation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasPercentage ] .

epo:DeliveryInformation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:specifiesGeneralDeliveryAgreement ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasDeliveryDeadline ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:specifiesSpecificDeliveryAgreement ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:concernsConsignee ] .

epo:Order a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:specifiesDeliveryInformation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasCustomerReference ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:comprisesOrderLine ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasPaymentTerm ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:specifiesSeller ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:specifiesOriginatorInformation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasAccountingCost ] .

