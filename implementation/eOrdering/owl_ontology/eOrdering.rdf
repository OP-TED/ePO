  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasCustomerReference">
    <skos:prefLabel xml:lang="en">Has customer reference</skos:prefLabel>
    <skos:definition xml:lang="en">A supplementary reference for the order. Additional information: This information can be used for the buyer's internal processes. This reference can also be used in the invoice. WG approval 2023-01-26 (epo-ord:Order)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#ord"/>
