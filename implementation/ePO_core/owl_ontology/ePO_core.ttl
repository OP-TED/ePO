	skos:prefLabel "Has medium tender per lots"@en .

a4g:hasMember a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:core ;
	skos:prefLabel "Has member"@en .

a4g:hasMinimumNumberOfCandidates a owl:DatatypeProperty ;
