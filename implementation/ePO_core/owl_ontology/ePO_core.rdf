    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasBuyerLegalType">
    <skos:prefLabel xml:lang="en">Has buyer legal type</skos:prefLabel>
    <skos:definition xml:lang="en">A category that indicates the right of an Organisation to play the role of a buyer. Additional Information: The category also effects the rules that the buyer has to abide to within the public procurement procedure. WG 07/09/2021 (org:Organization -&gt; at-voc:buyer-legal-type (+epo:hasBuyerLegalType))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasReceiptExpressionDeadline">
    <skos:prefLabel xml:lang="en">Has receipt expression deadline</skos:prefLabel>
    <skos:definition xml:lang="en">Time limit for receipt of expressions of interest. Pending of review by the WG (epo:SubmissionTerm)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/name">
    <skos:prefLabel xml:lang="en">Name</skos:prefLabel>
    <skos:definition xml:lang="en">The complete name of the Person as one string. (person:Person)</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#issuedByCertifier">
    <skos:prefLabel xml:lang="en">Issued by certifier</skos:prefLabel>
    <skos:definition xml:lang="en">Relation indicating the Certifier responsible for providing the ItemCertificate. Additional Information: Certifier is a role played by an organisation. WG approval 26/07/2022 (epo:Certificate -&gt; epo:Certifier (+epo:issuedByCertifier))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#definesLotGroup">
