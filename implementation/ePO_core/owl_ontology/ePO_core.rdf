    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasLegalBasisDescription">
    <skos:prefLabel xml:lang="en">Has legal basis description</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasAmountValue">
