    <skos:definition xml:lang="en">Incorporates AwardOutcome. (epo:AwardDecision -&gt; epo:AwardOutcome (+epo:comprisesAwardOutcome))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
