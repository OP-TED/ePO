    <skos:definition xml:lang="en">Relation indicating a ProcedureTerm has a LotGroup. (epo:ProcedureTerm -&gt; epo:LotGroup (+epo:definesLotGroup))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#isMultipleTenderSubmissionAllowed">
    <skos:prefLabel xml:lang="en">Is multiple tender submission allowed</skos:prefLabel>
    <skos:definition xml:lang="en">Tenderers may submit more than one competing tenders. WG Approval 10/10/2019 (epo:SubmissionTerm)</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#providesInformationOn">
    <skos:prefLabel xml:lang="en">Provides information on</skos:prefLabel>
    <skos:definition xml:lang="en">Offers information about an instance of a concept. WG approval 05/03/2024 (epo-sub:ESPD -&gt; org:Organization (+epo-sub:providesInformationOn))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
