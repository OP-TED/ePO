    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#hasTenderValidityDuration">
    <skos:prefLabel xml:lang="en">Has tender validity duration</skos:prefLabel>
