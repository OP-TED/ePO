    <rdfs:subClassOf rdf:resource="http://data.europa.eu/a4g/ontology#Estimate"/>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/a4g/ontology#includesTender">
    <skos:prefLabel xml:lang="en">Includes tender</skos:prefLabel>
    <skos:definition xml:lang="en">Additional information: This corresponds in eForms to BT-3202 Contract Tender Identifier. (epo:Contract -&gt; epo:Tender (+epo:includesTender))</skos:definition>
    <rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/ontology#core"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
