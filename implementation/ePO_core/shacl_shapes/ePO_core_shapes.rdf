	<rdf:Description rdf:about="http://data.europa.eu/a4g/data-shape/core/epo-AccessTerm-epo-isProcurementDocumentRestricted">
		<rdf:type rdf:resource="http://www.w3.org/ns/shacl#PropertyShape"/>
