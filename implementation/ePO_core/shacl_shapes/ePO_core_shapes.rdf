		<sh:path rdf:resource="http://data.europa.eu/a4g/ontology#isProcurementDocumentRestricted"/>
		<sh:description>The access to certain procurement documents is restricted. Additional Information: This corresponds in eForms to BT-14 Documents Restricted. WG Approval 10/10/2019</sh:description>
		<sh:name>Is procurement document restricted</sh:name>
		<sh:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</sh:minCount>
		<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
		<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
	</rdf:Description>

	<rdf:Description rdf:about="http://data.europa.eu/a4g/data-shape/core/epo-AccessTerm-epo-hasAdditionalInformationDeadline">
		<rdf:type rdf:resource="http://www.w3.org/ns/shacl#PropertyShape"/>
		<rdfs:isDefinedBy rdf:resource="http://data.europa.eu/a4g/data-shape/core/"/>
		<sh:path rdf:resource="http://data.europa.eu/a4g/ontology#hasAdditionalInformationDeadline"/>
		<sh:description>The time limit for requesting further information. Additional information: This is generally used to indicate the deadline for Economic Operators to request further information of the procedure before the submission deadline. This corresponds in eForms to BT-13 Additional Information Deadline. WG Approval 09/06/2020 </sh:description>
		<sh:name>Has additional information deadline</sh:name>
		<sh:minCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</sh:minCount>
		<sh:maxCount rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</sh:maxCount>
		<sh:datatype rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
	</rdf:Description>

