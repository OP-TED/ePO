	rdfs:isDefinedBy <http://data.europa.eu/a4g/data-shape/core/> ;
	sh:path a4g:isJointProcurement ;
	sh:description "Multiple buyers procure together within the same procedure. Addition Information: In case the joint procurement involves buyers from different countries, the national law is specified by the epo:hasCrossBorderLaw in the ProcedureTerm . " ;
	sh:name "Is joint procurement" ;
	sh:minCount 0 ;
	sh:maxCount 1 ;
	sh:datatype xsd:boolean .

core-shape:epo-Procedure-epo-isAccelerated a sh:PropertyShape ;
	rdfs:isDefinedBy <http://data.europa.eu/a4g/data-shape/core/> ;
