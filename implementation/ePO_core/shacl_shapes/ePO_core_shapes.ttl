        core-shape:epo-ParticipationRequestTerm-epo-definesParticipationRequestReceiver ;
    sh:targetClass :ParticipationRequestTerm .

core-shape:epo-PaymentExecutor a sh:NodeShape ;
    rdfs:label "Payment executor" ;
    rdfs:comment "A Role of an Agent responsible for executing the Payment." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass :PaymentExecutor .

core-shape:epo-Period a sh:NodeShape ;
    rdfs:label "Period" ;
