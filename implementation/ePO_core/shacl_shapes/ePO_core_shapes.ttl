    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:epo-ReviewIrregularitySummary-epo-hasIrregularityType,
        core-shape:epo-ReviewIrregularitySummary-epo-hasReviewIrregularityCount ;
    sh:targetClass :ReviewIrregularitySummary .

core-shape:epo-ReviewObject a sh:NodeShape ;
