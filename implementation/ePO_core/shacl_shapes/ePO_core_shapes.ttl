    sh:targetClass :ParticipationConditionsSummary .

core-shape:epo-ParticipationRequestProcessor a sh:NodeShape ;
    rdfs:label "Participation request processor" ;
    rdfs:comment "A Role of an Agent responsible for processing Requests To Participate." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass :ParticipationRequestProcessor .

core-shape:epo-ParticipationRequestReceiver a sh:NodeShape ;
    rdfs:label "Participation request receiver" ;
    rdfs:comment "A Role of an Agent responsible for receiving Requests To Participate." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass :ParticipationRequestReceiver .

core-shape:epo-ParticipationRequestTerm a sh:NodeShape ;
