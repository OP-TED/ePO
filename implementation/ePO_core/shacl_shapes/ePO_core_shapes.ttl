	sh:property core-shape:epo-Procedure-epo-isDesignContest, core-shape:epo-Procedure-epo-isJointProcurement, core-shape:epo-Procedure-epo-isAccelerated, core-shape:epo-Procedure-epo-hasAcceleratedProcedureJustification, core-shape:epo-Procedure-epo-hasMainFeature, core-shape:epo-Procedure-epo-involvesBuyer, core-shape:epo-Procedure-epo-isExecutedByProcurementServiceProvider, core-shape:epo-Procedure-epo-isSubjectToProcedureSpecificTerm, core-shape:epo-Procedure-epo-hasProcedureType, core-shape:epo-Procedure-epo-isResponsibilityOfBuyer, core-shape:epo-Procedure-epo-hasProcurementScopeDividedIntoLot, core-shape:epo-Procedure-epo-specifiesProcurementCriteriaSummary .

core-shape:epo-Procedure-epo-isDesignContest a sh:PropertyShape ;
	rdfs:isDefinedBy <http://data.europa.eu/a4g/data-shape/core/> ;
	sh:path a4g:isDesignContest ;
	sh:description "A competition which enables the buyer to acquire a plan or design via a jury. Additional information: Design contests have traditionally mostly been used in the fields of town and country planning, architecture and engineering or data processing, other purposes, such as to obtain plans for financial engineering The contest may include or not the award of prizes;" ;
	sh:name "Is design contest" ;
	sh:minCount 0 ;
	sh:maxCount 1 ;
	sh:datatype xsd:boolean .

