core-shape:epo-GreenProcurementShape a sh:NodeShape ;
	sh:targetClass a4g:GreenProcurement ;
	rdfs:isDefinedBy <http://data.europa.eu/a4g/data-shape/core/> ;
	rdfs:comment "Approach whereby Buyers seek to procure with a reduced Environmental Impact. Additional Information: The approach may apply to the complete life cycle. The reduced Environmental Impact is in comparison to goods, services and works with the same primary function that would otherwise be procured. Tightly related are article 68 - Life-cycle costing and article 67 - most economically advantageous Tender (see GPP handbook) https://ec.europa.eu/environment/gpp/pdf/Buying-Green-Handbook-3rd-Edition.pdf An instance of the class GreenProcurement is represented in eForms with the code env-imp defined in the codelist Strategic-Procurement.Paragraphs) https://ec.europa.eu/environment/gpp/pdf/Buying-Green-Handbook-3rd-Edition.pdf" ;
	sh:property core-shape:epo-GreenProcurement-epo-usesCleanVehicleDirective, core-shape:epo-GreenProcurement-epo-fulfillsRequirement .

core-shape:epo-GreenProcurement-epo-usesCleanVehicleDirective a sh:PropertyShape ;
	rdfs:isDefinedBy <http://data.europa.eu/a4g/data-shape/core/> ;
	sh:path a4g:usesCleanVehicleDirective ;
	sh:description "The procurement falls within the scope of the European Parliament and Council 2009/33/EC (Clean Vehicles Directive – CVD)." ;
	sh:name "Uses clean vehicle directive" ;
	sh:minCount 0 ;
	sh:maxCount 1 ;
	sh:datatype xsd:boolean .

core-shape:epo-GreenProcurement-epo-fulfillsRequirement a sh:PropertyShape ;
	rdfs:isDefinedBy <http://data.europa.eu/a4g/data-shape/core/> ;
	sh:path a4g:fulfillsRequirement ;
	sh:description "The requirement to which the concept meets. " ;
	sh:name "Fulfills requirement" ;
	sh:nodeKind sh:IRI ;
	sh:node [
		rdf:type sh:NodeShape ;
		sh:property [
			sh:path skos:inScheme ;
			sh:hasValue atold:green-public-procurement-criteria ;
		] ;
	], [
		rdf:type sh:NodeShape ;
		sh:property [
			sh:path skos:inScheme ;
			sh:hasValue atold:environmental-impact ;
		] ;
	] .

