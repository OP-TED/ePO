        core-shape:epo-ReviewRequest-epo-hasReviewRequestFee,
        core-shape:epo-ReviewRequest-epo-hasWithdrawalDate,
        core-shape:epo-ReviewRequest-epo-hasWithdrawalReason,
        core-shape:epo-ReviewRequest-epo-isWithdrawn,
        core-shape:epo-ReviewRequest-epo-requestsRemedyType ;
