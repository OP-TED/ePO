@prefix : <http://data.europa.eu/a4g/ontology#> .
@prefix cccev: <http://data.europa.eu/m8g/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:CertificateInformation a owl:Class ;
    rdfs:isDefinedBy :sub ;
    rdfs:subClassOf :ContextDescription ;
    skos:definition "Information about a Certificate."@en ;
    skos:prefLabel "Certificate information"@en .

:ESPD a owl:Class ;
    rdfs:isDefinedBy :sub ;
    rdfs:subClassOf :Document ;
    skos:definition "A document conveying the fulfilment or not by the economic operator of the Exclusion Grounds and the Selection Criteria set out by the Buyer for a specific Procurement using an European Single Procurement Document (ESPD) Request."@en ;
    skos:prefLabel "E s p d"@en .

:LegalRepresentative a owl:Class ;
    rdfs:isDefinedBy :sub ;
    rdfs:subClassOf :AuxiliaryParty ;
    skos:definition "The Role of an Agent that can represent an Organization."@en ;
    skos:prefLabel "Legal representative"@en .

:Mandate a owl:Class ;
    rdfs:isDefinedBy :sub ;
    rdfs:subClassOf :Document ;
    skos:definition "An authorization to act as a representative of an Organization."@en ;
    skos:prefLabel "Mandate"@en .

:NationalPreQualificationData a owl:Class ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Data that describe the distinctive features or characteristics that qualify an economic operator to take part in a tendering process."@en ;
    skos:prefLabel "National pre qualification data"@en .

:PowerOfAttorney a owl:Class ;
    rdfs:isDefinedBy :sub ;
    rdfs:subClassOf :Document ;
    skos:definition "A legal document that grants someone the authority to make decisions on behalf of an Organization."@en ;
    skos:prefLabel "Power of attorney"@en .

:Response a owl:Class ;
    rdfs:isDefinedBy :sub ;
    skos:definition "An answer given to a question that is part of an ESPD Request."@en ;
    skos:prefLabel "Response"@en .

:actsOnBehalfOf a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:prefLabel "Acts on behalf of"@en .

:answersQuestion a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relation indicating that the Response replies to a question. (epo-sub:Response -> cccev:InformationRequirement (+epo-sub:answersQuestion))"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Answers question"@en .

:concernsOrganisation a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relates to Organization. (epo-sub:NationalPreQualificationData -> org:Organization (+epo-sub:concernsOrganisation))"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Concerns organisation"@en .

:coversAllSelectionCriteria a owl:DatatypeProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Indicator that the Certificate proves whether the Organization fulfils all Selection Criteria. (epo-sub:CertificateInformation)"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Covers all selection criteria"@en .

:hasMandate a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relation indicating that a Legal Representative has a Mandate. (epo-sub:LegalRepresentative -> epo-sub:Mandate (+epo-sub:hasMandate))"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Has mandate"@en .

:hasPowerOfAttorney a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relation indicating that a Legal Representative has a Power of Attorney. (epo-sub:LegalRepresentative -> epo-sub:PowerOfAttorney (+epo-sub:hasPowerOfAttorney))"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Has power of attorney"@en .

:isReinforcedBy a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relation indicating an Evidence that supports the Response. (epo-sub:Response -> cccev:Evidence (+epo-sub:isReinforcedBy))"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Is reinforced by"@en .

:isSupportedBy a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:prefLabel "Is supported by"@en .

:providesInformationOn a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Offers information about an instance of a concept. (epo-sub:ESPD -> org:Organization (+epo-sub:providesInformationOn))"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Provides information on"@en .

:providesOrganisationCompletedTaskDescription a owl:DatatypeProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Text describing the works, supplies or services executed, delivered or performed in a procurement project that can be used as an evidence for the classification of the Organization. (epo-sub:NationalPreQualificationData)"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Provides organisation completed task description"@en .

:providesOrganisationEmployeeQuantity a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "The number of people hired by the Organization. (epo-sub:NationalPreQualificationData -> epo:Quantity (+epo-sub:providesOrganisationEmployeeQuantity))"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Provides organisation employee quantity"@en .

:providesOrganisationFinancialCapability a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "A monetary amount representing the financial capability of the Organization. Additional information: Used to represent the general Turnover of the Organization (for statistical purposes). (epo-sub:NationalPreQualificationData -> epo:MonetaryValue (+epo-sub:providesOrganisationFinancialCapability))"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Provides organisation financial capability"@en .

:providesOrganisationPreQualificationListingIdentifier a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "The identifier assigned to an economic operator in a national pre-qualification system or official list. (epo-sub:NationalPreQualificationData -> adms:Identifier (+epo-sub:providesOrganisationPreQualificationListingIdentifier))"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Provides organisation pre qualification listing identifier"@en .

:refersToApplicablePeriod a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relation indicating the period to which the Response shall apply. (epo-sub:Response -> epo:Period (+epo-sub:refersToApplicablePeriod))"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Refers to applicable period"@en .

:relatesToESPDRequest a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Is about an European Single Procurement Document (ESPD) Request. (epo-sub:ESPD -> epo-acc:ESPDRequest (+epo-sub:relatesToESPDRequest))"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Relates to e s p d request"@en .

:specifiesResponse a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relation indicating that an ESPD Response contains a Response. (epo-sub:ESPD -> epo-sub:Response (+epo-sub:specifiesResponse))"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Specifies response"@en .

:summarisesInformationAboutCertificate a owl:ObjectProperty ;
    rdfs:isDefinedBy :sub ;
    skos:definition "Relation indicating the Certificate that the information refers to. (epo-sub:CertificateInformation -> epo:Certificate (+epo-sub:summarisesInformationAboutCertificate))"@en ;
    skos:historyNote "WG Approval 12/03/2024"@en ;
    skos:prefLabel "Summarises information about certificate"@en .

cccev:SupportedValue a owl:Class ;
    skos:definition "External concept defined by <xref scope=\"external\" href=\"http://data.europa.eu/m8g/SupportedValue\"><foreign otherprops=\"#0000ff\"><u>http://data.europa.eu/m8g/SupportedValue</u></foreign></xref>"@en ;
    skos:prefLabel "Supported value"@en .

cccev:confidentialityLevelType a owl:ObjectProperty ;
    skos:definition "External concept defined by <xref scope=\"external\" href=\"http://data.europa.eu/m8g/confidentialityLevelType\"><foreign otherprops=\"#0000ff\"><u>http://data.europa.eu/m8g/confidentialityLevelType</u></foreign></xref> (epo-sub:Response -> at-voc:access-rights (+cccev:confidentialityLevelType))"@en ;
    skos:prefLabel "Confidentiality level type"@en .

cccev:providesValueFor a owl:ObjectProperty ;
    skos:definition "External concept defined by <xref scope=\"external\" href=\"http://data.europa.eu/m8g/providesValueFor\"><foreign otherprops=\"#0000ff\"><u>http://data.europa.eu/m8g/providesValueFor</u></foreign></xref> (cccev:SupportedValue -> cccev:InformationConcept (+cccev:providesValueFor))"@en ;
    skos:prefLabel "Provides value for"@en .

cccev:supportsValue a owl:ObjectProperty ;
    skos:definition "External concept defined by <xref scope=\"external\" href=\"http://data.europa.eu/m8g/supportsValue\"><foreign otherprops=\"#0000ff\"><u>http://data.europa.eu/m8g/supportsValue</u></foreign></xref> (epo-sub:Response -> cccev:SupportedValue (+cccev:supportsValue)) External concept defined by <xref scope=\"external\" href=\"http://data.europa.eu/m8g/supportsValue\"><foreign otherprops=\"#0000ff\"><u>http://data.europa.eu/m8g/supportsValue</u></foreign></xref> (cccev:Evidence -> cccev:SupportedValue (+cccev:supportsValue))"@en ;
    skos:prefLabel "Supports value"@en .

cccev:value a owl:DatatypeProperty ;
    skos:definition "External concept defined by <xref scope=\"external\" href=\"http://data.europa.eu/m8g/value\"><foreign otherprops=\"#0000ff\"><u>http://data.europa.eu/m8g/value</u></foreign></xref> (cccev:SupportedValue)"@en ;
    skos:prefLabel "Value"@en .

dcterms:description a owl:DatatypeProperty ;
    skos:definition "External concept defined by <xref scope=\"external\" href=\"https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#http://purl.org/dc/terms/description\"><foreign otherprops=\"#0000ff\"><u>https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#http://purl.org/dc/terms/description</u></foreign></xref> (epo-sub:CertificateInformation)"@en ;
    skos:historyNote "WG Approval 30/05/2023"@en ;
    skos:prefLabel "Description"@en .

rdf:PlainLiteral a rdfs:Datatype ;
    skos:prefLabel "Plain literal"@en .

xsd:anyURI a rdfs:Datatype ;
    skos:prefLabel "Any u r i"@en .

xsd:boolean a rdfs:Datatype ;
    skos:prefLabel "Boolean"@en .

xsd:date a rdfs:Datatype ;
    skos:prefLabel "Date"@en .

xsd:dateTime a rdfs:Datatype ;
    skos:prefLabel "Date time"@en .

xsd:decimal a rdfs:Datatype ;
    skos:prefLabel "Decimal"@en .

xsd:integer a rdfs:Datatype ;
    skos:prefLabel "Integer"@en .

