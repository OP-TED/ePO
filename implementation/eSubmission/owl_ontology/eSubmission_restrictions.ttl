@prefix r2rml: <http://www.w3.org/ns/r2rml#> .
@prefix at: <http://publications.europa.eu/ontology/authority/> .
@prefix atold: <http://publications.europa.eu/resource/authority/> .
@prefix vann: <http://purl.org/vocab/vann/> .

a4g:actsOnBehalfOf rdfs:domain a4g:LegalRepresentative ;
	rdfs:range a4g:OfferingParty .

a4g:LegalRepresentative rdfs:subClassOf [
		rdf:type owl:Restriction ;
		owl:maxCardinality 1 ;
		owl:onProperty a4g:hasPowerOfAttorney ;
	] .

a4g:answersQuestion rdfs:domain a4g:Response ;
	rdfs:range m8g:InformationRequirement .

a4g:concernsOrganisation rdfs:domain a4g:NationalPreQualificationData ;
	rdfs:range org:Organization .

a4g:NationalPreQualificationData rdfs:subClassOf [
		rdf:type owl:Restriction ;
		owl:maxCardinality 1 ;
		owl:onProperty a4g:providesOrganisationCompletedTaskDescription ;
	] .

a4g:hasMandate rdfs:domain a4g:LegalRepresentative ;
	rdfs:range a4g:Mandate .

a4g:isReinforcedBy rdfs:domain a4g:Response ;
	rdfs:range m8g:Evidence .

a4g:isSupportedBy rdfs:domain a4g:Tender ;
	rdfs:range a4g:ESPD .

a4g:ESPD rdfs:subClassOf [
		rdf:type owl:Restriction ;
		owl:onProperty a4g:specifiesResponse ;
		owl:minCardinality 1 ;
	] .

a4g:providesInformationOn rdfs:domain a4g:ESPD ;
	rdfs:range org:Organization .

a4g:providesOrganisationEmployeeQuantity rdfs:domain a4g:NationalPreQualificationData ;
	rdfs:range a4g:Quantity .

a4g:providesOrganisationFinancialCapability rdfs:domain a4g:NationalPreQualificationData ;
	rdfs:range a4g:MonetaryValue .

a4g:providesOrganisationPreQualificationListingIdentifier rdfs:domain a4g:NationalPreQualificationData ;
	rdfs:range adms:Identifier .

a4g:refersToApplicablePeriod rdfs:domain a4g:Response ;
	rdfs:range a4g:Period .

a4g:relatesToESPDRequest rdfs:domain a4g:ESPD ;
	rdfs:range a4g:ESPDRequest .

a4g:sub-restriction a owl:Ontology ;
	rdfs:label "eProcurement Ontology Submission - core restrictions"@en ;
	dcterms:created "2021-06-01"^^xsd:date ;
	dcterms:description "The eProcurement Ontology Submission core restrictions provides the restrictions and the inference-related specifications on the concepts and properties in the eProcurement Ontology Submission core."@en ;
	dcterms:issued "2024-11-12"^^xsd:date ;
	dcterms:license "Â© European Union, 2014. Unless otherwise noted, the reuse of the Ontology is authorised under the European Union Public Licence v1.2 (https://eupl.eu/)." ;
	dcterms:publisher "http://publications.europa.eu/resource/authority/corporate-body/PUBL" ;
	dcterms:title "eProcurement Ontology Submission - core restrictions"@en ;
	vann:preferredNamespacePrefix "epo" ;
	vann:preferredNamespaceUri "http://data.europa.eu/a4g/ontology#" ;
	rdfs:comment """This version is automatically generated from eSubmission.xml on 
                2024-11-12""" ;
	rdfs:seeAlso <https://docs.ted.europa.eu/home/index.html>, <https://github.com/OP-TED/ePO/releases>, <https://joinup.ec.europa.eu/collection/eprocurement/solution/eprocurement-ontology/about>, <https://op.europa.eu/en/web/eu-vocabularies/e-procurement> ;
	owl:imports a4g:acc, a4g:core, a4g:sub, <http://data.europa.eu/eli/ontology#>, <http://data.europa.eu/m8g/>, <http://iflastandards.info/ns/fr/frbr/frbroo/>, <http://purl.org/dc/terms/>, <http://purl.org/vocab/vann/>, <http://www.w3.org/2004/02/skos/core>, <http://www.w3.org/2006/time>, <http://www.w3.org/ns/adms>, <http://www.w3.org/ns/locn>, <http://www.w3.org/ns/org#>, <http://www.w3.org/ns/person>, <http://xmlns.com/foaf/0.1/> ;
	owl:incompatibleWith "3.1.0" ;
	owl:priorVersion "http://data.europa.eu/a4g/ontology#sub-restriction-4.1.1" ;
	owl:versionIRI a4g:sub-restriction-4.2.0 ;
	owl:versionInfo "4.2.0" .

a4g:summarisesInformationAboutCertificate rdfs:domain a4g:CertificateInformation ;
	rdfs:range a4g:Certificate .

a4g:CertificateInformation rdfs:subClassOf [
		rdf:type owl:Restriction ;
		owl:maxCardinality 1 ;
		owl:onProperty a4g:coversAllSelectionCriteria ;
	], [
		rdf:type owl:Restriction ;
		owl:maxCardinality 1 ;
		owl:onProperty dcterms:description ;
	] .

a4g:coversAllSelectionCriteria rdfs:domain a4g:CertificateInformation ;
	rdfs:range xsd:boolean .

a4g:hasPowerOfAttorney rdfs:domain a4g:LegalRepresentative ;
	rdfs:range a4g:PowerOfAttorney .

a4g:providesOrganisationCompletedTaskDescription rdfs:domain a4g:NationalPreQualificationData ;
	rdfs:range rdf:PlainLiteral .

a4g:specifiesResponse rdfs:domain a4g:ESPD ;
	rdfs:range a4g:Response .

_:b9 a owl:AllDisjointClasses ;
	owl:members (
		a4g:ESPD
		a4g:Mandate
		a4g:PowerOfAttorney
	) .

