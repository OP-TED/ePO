@prefix : <http://data.europa.eu/a4g/ontology#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix cccev: <http://data.europa.eu/m8g/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:actsOnBehalfOf rdfs:domain :LegalRepresentative ;
    rdfs:range :OfferingParty .

:answersQuestion rdfs:domain :Response ;
    rdfs:range cccev:InformationRequirement .

:concernsOrganisation rdfs:domain :NationalPreQualificationData ;
    rdfs:range org:Organization .

:hasMandate rdfs:domain :LegalRepresentative ;
    rdfs:range :Mandate .

:isReinforcedBy rdfs:domain :Response ;
    rdfs:range cccev:Evidence .

:isSupportedBy rdfs:domain :Tender ;
    rdfs:range :ESPD .

:providesInformationOn rdfs:domain :ESPD ;
    rdfs:range org:Organization .

:providesOrganisationEmployeeQuantity rdfs:domain :NationalPreQualificationData ;
    rdfs:range :Quantity .

:providesOrganisationFinancialCapability rdfs:domain :NationalPreQualificationData ;
    rdfs:range :MonetaryValue .

:providesOrganisationPreQualificationListingIdentifier rdfs:domain :NationalPreQualificationData ;
    rdfs:range adms:Identifier .

:refersToApplicablePeriod rdfs:domain :Response ;
    rdfs:range :Period .

:relatesToESPDRequest rdfs:domain :ESPD ;
    rdfs:range :ESPDRequest .

:sub-restriction a owl:Ontology ;
    rdfs:label "eProcurement Ontology Submission - core restrictions"@en ;
    dcterms:created "2021-06-01"^^xsd:date ;
    dcterms:description "The eProcurement Ontology Submission core restrictions provides the restrictions and the inference-related specifications on the concepts and properties in the eProcurement Ontology Submission core."@en ;
    dcterms:issued "2024-07-18"^^xsd:date ;
    dcterms:license "Â© European Union, 2014. Unless otherwise noted, the reuse of the Ontology is authorised under the European Union Public Licence v1.2 (https://eupl.eu/)." ;
    dcterms:publisher "http://publications.europa.eu/resource/authority/corporate-body/PUBL" ;
    dcterms:title "eProcurement Ontology Submission - core restrictions"@en ;
    vann:preferredNamespacePrefix "epo" ;
    vann:preferredNamespaceUri "http://data.europa.eu/a4g/ontology#" ;
    rdfs:comment """This version is automatically generated from eSubmission.xml on 
                2024-07-18""" ;
    rdfs:seeAlso <https://docs.ted.europa.eu/home/index.html>,
        <https://github.com/OP-TED/ePO/releases>,
        <https://joinup.ec.europa.eu/collection/eprocurement/solution/eprocurement-ontology/about>,
        <https://op.europa.eu/en/web/eu-vocabularies/e-procurement> ;
    owl:imports :acc,
        :core,
        :sub,
        eli:,
        cccev:,
        frbroo:,
        dcterms:,
        vann:,
        <http://www.w3.org/2004/02/skos/core>,
        <http://www.w3.org/2006/time>,
        <http://www.w3.org/ns/adms>,
        <http://www.w3.org/ns/locn>,
        org:,
        <http://www.w3.org/ns/person>,
        foaf: ;
    owl:incompatibleWith "3.1.0" ;
    owl:priorVersion "http://data.europa.eu/a4g/ontology#sub-restriction-4.0.2" ;
    owl:versionIRI :sub-restriction-4.1.0 ;
    owl:versionInfo "4.1.0" .

:summarisesInformationAboutCertificate rdfs:domain :CertificateInformation ;
    rdfs:range :Certificate .

cccev:confidentialityLevelType rdfs:domain :Response ;
    rdfs:range skos:Concept .

cccev:providesValueFor rdfs:domain cccev:SupportedValue ;
    rdfs:range cccev:InformationConcept .

cccev:supportsValue rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Response cccev:Evidence ) ] ;
    rdfs:range cccev:SupportedValue .

cccev:value rdfs:domain cccev:SupportedValue ;
    rdfs:range rdf:PlainLiteral .

:coversAllSelectionCriteria rdfs:domain :CertificateInformation ;
    rdfs:range xsd:boolean .

:hasPowerOfAttorney rdfs:domain :LegalRepresentative ;
    rdfs:range :PowerOfAttorney .

:providesOrganisationCompletedTaskDescription rdfs:domain :NationalPreQualificationData ;
    rdfs:range rdf:PlainLiteral .

:specifiesResponse rdfs:domain :ESPD ;
    rdfs:range :Response .

dcterms:description rdfs:domain :CertificateInformation ;
    rdfs:range rdf:PlainLiteral .

:CertificateInformation rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :coversAllSelectionCriteria ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcterms:description ] .

:LegalRepresentative rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPowerOfAttorney ] .

:ESPD rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :specifiesResponse ] .

:NationalPreQualificationData rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :providesOrganisationCompletedTaskDescription ] .

[] a owl:AllDisjointClasses ;
    owl:members ( :ESPD :Mandate :PowerOfAttorney ) .

