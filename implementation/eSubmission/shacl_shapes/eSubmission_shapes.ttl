a4g_shape:PlainLiteral a shacl:NodeShape ;
	shacl:or (
		[
			shacl:datatype xsd:string ;
		]
		[
			shacl:datatype rdf:langString ;
		]
	) .

sub-shape:NationalPreQualificationDataShape a shacl:NodeShape ;
	shacl:targetClass a4g:NationalPreQualificationData ;
	rdfs:isDefinedBy <http://data.europa.eu/a4g/data-shape/sub/> ;
	rdfs:comment "Data that describe the distinctive features or characteristics that qualify an economic operator to take part in a tendering process. WG approval 05/03/2024" ;
	shacl:property sub-shape:NationalPreQualificationData-providesOrganisationCompletedTaskDescription-PlainLiteral, sub-shape:NationalPreQualificationData-concernsOrganisation-Organization, sub-shape:NationalPreQualificationData-providesOrganisationEmployeeQuantity-Quantity, sub-shape:NationalPreQualificationData-providesOrganisationFinancialCapability-MonetaryValue, sub-shape:NationalPreQualificationData-providesOrganisationPreQualificationListingIdentifier-Identifier .

