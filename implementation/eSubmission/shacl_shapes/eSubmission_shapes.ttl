sub-shape:cccev-SupportedValue a sh:NodeShape ;
    rdfs:label "Supported value" ;
    rdfs:comment "Value for an Information Concept that is provided by an Evidence. Additional Information: The notion of Supported Value is closely related to actual data exchange between two parties: (a) the Requirement processor, i.e. the Agent setting out Requirements for an objective and processing the supplied Evidences in the context of the Requirements, and (b) the Evidence provider, i.e. the Agent supplying information to an information request expressed as Requirements. The Requirement processor has expressed its expectations (both business as technical) for the information it wants to recieve as an Information Concept. The Evidence provider is able to supply information for that Information Concept, but its native data representation might not be coherent with the expectations set by the Requirement processor. The Supported Value is bridging both. The Evidence provider can either provide a derived value (fact) from its native data representation that complies with the Information Concept expectations. Or it can provide a query in an agreed language between Evidence provider and Requirement processor that allows the Requirement processor to retrieve the value from the native data representation. Implementers are free to choose their language. It is recommended to document the made agreements well." ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:property sub-shape:cccev-SupportedValue-cccev-value ;
    sh:targetClass cccev:SupportedValue .

sub-shape:epo-sub-CertificateInformation a sh:NodeShape ;
    rdfs:label "Certificate information" ;
    rdfs:comment "Information about a Certificate. WG approval 12/03/2024" ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:property sub-shape:epo-sub-CertificateInformation-dct-description,
        sub-shape:epo-sub-CertificateInformation-epo-sub-coversAllSelectionCriteria ;
    sh:targetClass :CertificateInformation .

sub-shape:epo-sub-ESPD a sh:NodeShape ;
    rdfs:label "E s p d" ;
    rdfs:comment "A document conveying the fulfilment or not by the economic operator of the Exclusion Grounds and the Selection Criteria set out by the Buyer for a specific Procurement using an European Single Procurement Document (ESPD) Request. WG Approval 05/03/2024" ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:property sub-shape:epo-sub-ESPD-epo-sub-refersToOtherESPDResponse,
        sub-shape:epo-sub-ESPD-epo-sub-specifiesResponse ;
    sh:targetClass :ESPD .

sub-shape:epo-sub-LegalRepresentative a sh:NodeShape ;
    rdfs:label "Legal representative" ;
    rdfs:comment "The Role of an Agent that can represent an Organization. WG approval 05/03/2024" ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:property sub-shape:epo-sub-LegalRepresentative-epo-sub-hasMandate,
        sub-shape:epo-sub-LegalRepresentative-epo-sub-hasPowerOfAttorney ;
    sh:targetClass :LegalRepresentative .

sub-shape:epo-sub-Mandate a sh:NodeShape ;
    rdfs:label "Mandate" ;
    rdfs:comment "An authorization to act as a representative of an Organization. WG approval 05/03/2024" ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:targetClass :Mandate .

sub-shape:epo-sub-NationalPreQualificationData a sh:NodeShape ;
    rdfs:label "National pre qualification data" ;
    rdfs:comment "Data that describe the distinctive features or characteristics that qualify an economic operator to take part in a tendering process. WG approval 05/03/2024" ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:property sub-shape:epo-sub-NationalPreQualificationData-epo-sub-providesOrganisationCompletedTaskDescription ;
    sh:targetClass :NationalPreQualificationData .

sub-shape:epo-sub-PowerOfAttorney a sh:NodeShape ;
    rdfs:label "Power of attorney" ;
    rdfs:comment "A legal document that grants someone the authority to make decisions on behalf of an Organization. WG approval 05/03/2024" ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:targetClass :PowerOfAttorney .

sub-shape:epo-sub-Response a sh:NodeShape ;
    rdfs:label "Response" ;
    rdfs:comment "An answer given to a question that is part of an ESPD Request. WG approval 12/03/2024" ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:property sub-shape:epo-sub-Response-cccev-supportsValue ;
    sh:targetClass :Response .

sub-shape:cccev-SupportedValue-cccev-value a sh:PropertyShape ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "Value for the Information Concept that the Evidence supports." ;
    sh:name "Value" ;
    sh:path cccev:value .

sub-shape:epo-sub-CertificateInformation-dct-description a sh:PropertyShape ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "An account of the resource. Additional Information: Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource. WG Approval 30/05/2023" ;
    sh:maxCount 1 ;
    sh:name "Description" ;
    sh:path dcterms:description .

sub-shape:epo-sub-CertificateInformation-epo-sub-coversAllSelectionCriteria a sh:PropertyShape ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:datatype xsd:boolean ;
    sh:description "Indicator that the Certificate proves whether the Organization fulfils all Selection Criteria. WG approval 12/03/2024" ;
    sh:maxCount 1 ;
    sh:name "Covers all selection criteria" ;
    sh:path :coversAllSelectionCriteria .

sub-shape:epo-sub-ESPD-epo-sub-refersToOtherESPDResponse a sh:PropertyShape ;
    sh:class :ESPD ;
    sh:description "Reference to other European Single Procurement Document (ESPD) Response. WG approval 05/03/2024" ;
    sh:name "Refers to other e s p d response" ;
    sh:path :refersToOtherESPDResponse ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#refersToOtherESPDResponse> ?that . ?that <http://data.europa.eu/a4g/ontology#refersToOtherESPDResponse> ?this .}" ] .

sub-shape:epo-sub-ESPD-epo-sub-specifiesResponse a sh:PropertyShape ;
    sh:class :Response ;
    sh:description "Relation indicating that an ESPD Response contains a Response. WG approval 12/03/2024" ;
    sh:minCount 1 ;
    sh:name "Specifies response" ;
    sh:path :specifiesResponse ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#specifiesResponse> ?that . ?that <http://data.europa.eu/a4g/ontology#specifiesResponse> ?this .}" ] .

sub-shape:epo-sub-LegalRepresentative-epo-sub-hasMandate a sh:PropertyShape ;
    sh:class :Mandate ;
    sh:description "Relation indicating that a Legal Representative has a Mandate. WG approval 12/03/2024" ;
    sh:name "Has mandate" ;
    sh:path :hasMandate ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasMandate> ?that . ?that <http://data.europa.eu/a4g/ontology#hasMandate> ?this .}" ] .

sub-shape:epo-sub-LegalRepresentative-epo-sub-hasPowerOfAttorney a sh:PropertyShape ;
    sh:class :PowerOfAttorney ;
    sh:description "Relation indicating that a Legal Representative has a Power of Attorney. WG approval 12/03/2024" ;
    sh:maxCount 1 ;
    sh:name "Has power of attorney" ;
    sh:path :hasPowerOfAttorney ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasPowerOfAttorney> ?that . ?that <http://data.europa.eu/a4g/ontology#hasPowerOfAttorney> ?this .}" ] .

sub-shape:epo-sub-NationalPreQualificationData-epo-sub-providesOrganisationCompletedTaskDescription a sh:PropertyShape ;
    rdfs:isDefinedBy sub-shape:sub-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "Text describing the works, supplies or services executed, delivered or performed in a procurement project that can be used as an evidence for the classification of the Organization. WG approval 05/03/2024" ;
    sh:maxCount 1 ;
    sh:name "Provides organisation completed task description" ;
    sh:path :providesOrganisationCompletedTaskDescription .

sub-shape:epo-sub-Response-cccev-supportsValue a sh:PropertyShape ;
    sh:class cccev:SupportedValue ;
    sh:description "Supported Value that the Evidence contains." ;
    sh:name "Supports value" ;
    sh:path cccev:supportsValue ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/m8g/supportsValue> ?that . ?that <http://data.europa.eu/m8g/supportsValue> ?this .}" ] .

