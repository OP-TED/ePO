            owl:cardinality 1 ;
            owl:onProperty :specifiesItem ] .

:Batch rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasExpiryDate ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
