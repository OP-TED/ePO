            owl:maxCardinality 1 ;
            owl:onProperty :hasOrderableQuantityIncrement ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :hasPrice ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :isContractedItem ] .

