:hasInvoiceNoteDescription a owl:FunctionalProperty ;
    rdfs:domain :InvoiceNote ;
    rdfs:range rdf:PlainLiteral .

:hasPaymentAccountName rdfs:domain :CreditTransferInformation ;
    rdfs:range rdf:PlainLiteral .

:hasPaymentCardHolderName rdfs:domain :PaymentCardInformation ;
    rdfs:range rdf:PlainLiteral .

:hasPaymentCardInformation rdfs:domain :PaymentInstructionsInformation ;
    rdfs:range :PaymentCardInformation .

:hasPaymentCardPrimaryAccountNumber a owl:FunctionalProperty ;
    rdfs:domain :PaymentCardInformation ;
    rdfs:range rdf:PlainLiteral .

:hasPaymentDueDate rdfs:domain :Invoice ;
    rdfs:range xsd:date .

:hasPaymentInstructionsInformation rdfs:domain :Invoice ;
    rdfs:range :PaymentInstructionsInformation .

:hasPaymentMeansDescription rdfs:domain :PaymentInstructionsInformation ;
    rdfs:range rdf:PlainLiteral .

:hasPaymentTerm rdfs:domain :Invoice ;
    rdfs:range rdf:PlainLiteral .

