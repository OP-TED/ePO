    rdfs:range xsd:date .

:isSubmittedForInvoiceLine rdfs:domain :CreditNoteLine ;
    rdfs:range :InvoiceLine .

:refersToInvoice rdfs:domain :CreditNote ;
    rdfs:range :Invoice .

:specifiesPayee rdfs:domain :Invoice ;
    rdfs:range :Payee .

:specifiesSellerTaxRepresentative rdfs:domain :Invoice ;
    rdfs:range :SellerTaxRepresentative .

:CreditNote rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :refersToInvoice ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :comprisesCreditNoteLine ] .

:CreditNoteLine rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :isSubmittedForInvoiceLine ] .

:hasAccountingCost rdfs:domain [ a owl:Class ;
            owl:unionOf ( :Invoice :InvoiceLine ) ] ;
    rdfs:range rdf:PlainLiteral .

:InvoiceNote rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :hasInvoiceNoteDescription ] .

:PaymentCardInformation rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :hasPaymentCardPrimaryAccountNumber ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPaymentCardHolderName ] .

:TaxBreakdownInformation rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasTaxExemptionReasonDescription ] .

:CreditTransferInformation rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPaymentAccountName ] .

:PaymentInstructionsInformation rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasRemittanceInformation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPaymentMeansDescription ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPaymentCardInformation ] .

:InvoiceLine a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasAccountingCost ] .

:Invoice a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasCustomerReference ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :comprisesInvoiceLine ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :specifiesPayee ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :hasTaxBreakdownInformation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPaymentDueDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPaymentTerm ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :specifiesSellerTaxRepresentative ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasTaxPointDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasAccountingCost ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasPaymentInstructionsInformation ] .

[] a owl:AllDisjointClasses ;
    owl:members ( :CreditTransferInformation :PaymentCardInformation :PaymentInstructionsInformation ) .

[] a owl:AllDisjointClasses ;
    owl:members ( :Payee :SellerTaxRepresentative ) .

