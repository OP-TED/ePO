:isSpecificToInvoiceLine rdfs:domain [ a owl:Class ;
            owl:unionOf ( :ChargeInformation :AllowanceInformation ) ] ;
    rdfs:range :InvoiceLine .

:refersToContract rdfs:domain :Invoice ;
    rdfs:range :Contract .

:refersToDespatchAdvice rdfs:domain :Invoice ;
    rdfs:range :DespatchAdvice .

:refersToOrder rdfs:domain :Invoice ;
    rdfs:range :Order .

:refersToOrderLine rdfs:domain :InvoiceLine ;
    rdfs:range :OrderLine .

:refersToProject rdfs:domain :Invoice ;
    rdfs:range :Project .

:refersToReceiptAdvice rdfs:domain :Invoice ;
    rdfs:range :ReceiptAdvice .

:specifiesAllowanceInformation rdfs:domain :Invoice ;
    rdfs:range :AllowanceInformation .

:specifiesBuyer rdfs:domain :Invoice ;
    rdfs:range :Buyer .

:specifiesChargeInformation rdfs:domain :Invoice ;
    rdfs:range :ChargeInformation .

:specifiesDeliveryInformation rdfs:domain :Invoice ;
