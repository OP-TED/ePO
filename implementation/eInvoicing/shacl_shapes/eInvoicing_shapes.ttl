    rdfs:isDefinedBy inv-shape:inv-shape ;
    sh:property inv-shape:epo-inv-Invoice-epo-inv-comprisesInvoiceLine,
        inv-shape:epo-inv-Invoice-epo-inv-hasInvoiceNote,
