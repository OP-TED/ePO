    rdfs:isDefinedBy inv-shape:inv-shape ;
    sh:property inv-shape:epo-inv-CreditNote-epo-inv-comprisesCreditNoteLine,
        inv-shape:epo-inv-CreditNote-epo-inv-refersToInvoice ;
    sh:targetClass :CreditNote .

inv-shape:epo-inv-CreditNoteLine a sh:NodeShape ;
