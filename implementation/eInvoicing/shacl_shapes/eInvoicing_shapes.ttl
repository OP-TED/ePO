        inv-shape:epo-inv-Invoice-epo-ord-hasCustomerReference,
        inv-shape:epo-inv-Invoice-epo-ord-hasPaymentTerm ;
    sh:targetClass :Invoice .

inv-shape:epo-inv-Invoice-epo-inv-hasInvoicedObjectIdentifier sh:description "check https://skr.se/download/18.42336a32177c8ab158d77219/1616148012158/Referencing_and_Matching_3.pdf" .

