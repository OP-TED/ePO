        inv-shape:epo-inv-Invoice-epo-inv-hasPaymentInstructionsInformation,
        inv-shape:epo-inv-Invoice-epo-inv-hasTaxBreakdownInformation,
        inv-shape:epo-inv-Invoice-epo-inv-hasTaxPointDate,
        inv-shape:epo-inv-Invoice-epo-inv-hasTaxPointDateCode,
