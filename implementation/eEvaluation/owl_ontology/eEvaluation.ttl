:Bidder a owl:Class ;
    rdfs:isDefinedBy :core ;
    rdfs:subClassOf :Tenderer ;
    skos:prefLabel "Bidder"@en .

:EvaluationBoard a owl:Class ;
    rdfs:isDefinedBy :core ;
    skos:definition "Committee set up to evaluate the Tenders submitted. Additional Information: The committee may be a regular board or a jury."@en ;
    skos:editorialNote "if an Evaluation Committee has Jury Members, then it is for a Design Contest."@en ;
    skos:historyNote "WG approval 13/04/2021"@en ;
    skos:prefLabel "Evaluation Board"@en .

:EvaluationCriterion a owl:Class ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "Evaluation Criterion"@en .

:EvaluationReport a owl:Class ;
    rdfs:isDefinedBy :core ;
    rdfs:subClassOf :Document ;
    skos:prefLabel "Evaluation Report"@en .

:Evaluator a owl:Class ;
    rdfs:isDefinedBy :core ;
    rdfs:subClassOf :AcquiringParty ;
    skos:prefLabel "Evaluator"@en .

:ExcludedCandidateList a owl:Class ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "Excluded Candidate List"@en .

:ForeseenSubcontractor a owl:Class ;
    rdfs:isDefinedBy :core ;
    rdfs:subClassOf :OfferingParty ;
    skos:definition "NotReliedUponEntity can be an alias."@en ;
    skos:prefLabel "Foreseen Subcontractor"@en .

:TenderClarification a owl:Class ;
    rdfs:isDefinedBy :core ;
    rdfs:subClassOf :ProcurementDocument ;
    skos:prefLabel "Tender Clarification"@en .

:TenderClarificationRequest a owl:Class ;
    rdfs:isDefinedBy :core ;
    rdfs:subClassOf :Document ;
    skos:prefLabel "Tender Clarification Request"@en .

:UniqueResponsibleOfTheProcurement a owl:Class ;
    rdfs:isDefinedBy :core ;
    rdfs:subClassOf :AcquiringParty ;
    skos:prefLabel "Unique Responsible Of The Procurement"@en .

:concernsEvaluationReport a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "concerns Evaluation Report"@en .

:containsCandidate a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "contains Candidate"@en .

:evaluatesProcurementCriterion a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "evaluates Procurement Criterion"@en .

:fulfillsProcurementCriterion a owl:DatatypeProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "fulfills Procurement Criterion"@en .

:hasAwardCriteriaEvaluationScore a owl:DatatypeProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "has Award Criteria Evaluation Score"@en .

:hasEvaluationJustification a owl:DatatypeProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "has Evaluation Justification"@en .

:hasEvaluator a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "has Evaluator"@en .

:hasJuryMember a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:definition "An agent involved in an evaluation board."@en ;
    skos:historyNote "WG Approval 14/09/2021"@en ;
    skos:prefLabel "has Jury Member"@en .

:hasScorePoint a owl:DatatypeProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "has Score Point"@en .

:isAppointedBy a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "is Appointed By"@en .

:isAssignedForEvaluationOf a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "is Assigned For Evaluation Of"@en .

:playedBy a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "played By"@en .

:providesRankToTender a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "provides Rank To Tender"@en .

:refersToTenderClarificationRequest a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "refers To Tender Clarification Request"@en .

:reportsEvaluationCriterion a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "reports Evaluation Criterion"@en .

:requestsClarification a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "requests Clarification"@en .

:specifiesEconomicOperator a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "specifies Economic Operator"@en .

:usesEvaluationFormula a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "uses Evaluation Formula"@en .

:writesEvaluationReport a owl:ObjectProperty ;
    rdfs:isDefinedBy :core ;
    skos:prefLabel "writes Evaluation Report"@en .

cccev:hasRequirement a owl:ObjectProperty ;
