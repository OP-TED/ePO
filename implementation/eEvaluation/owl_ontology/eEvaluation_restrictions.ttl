    owl:imports :core,
        <http://data.europa.eu/nuts>,
        dcterms:,
        skos:,
        <http://www.w3.org/ns/adms>,
        foaf:,
        <https://data.europa.eu/m8g> ;
    owl:incompatibleWith "2.1.0" ;
    owl:priorVersion "http://data.europa.eu/a4g/ontology#core-restriction-4.2.0" ;
