:hasEvaluator rdfs:domain :EvaluationBoard ;
    rdfs:range :Evaluator .

:hasJuryMember rdfs:domain :EvaluationBoard ;
    rdfs:range :JuryMember .

:isAssignedForEvaluationOf rdfs:domain :EvaluationBoard ;
    rdfs:range :Lot .

:requestsClarification rdfs:domain :EvaluationBoard ;
    rdfs:range :TenderClarificationRequest .

:TenderClarification rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :refersToTenderClarificationRequest ] .

:concernsEvaluationReport rdfs:domain :TenderClarificationRequest ;
    rdfs:range :EvaluationReport .

:evaluatesProcurementCriterion a owl:FunctionalProperty ;
    rdfs:domain :EvaluationCriterion ;
    rdfs:range :ProcurementCriterion .

:fulfillsProcurementCriterion rdfs:domain :EvaluationCriterion ;
    rdfs:range xsd:boolean .

:hasAwardCriteriaEvaluationScore rdfs:domain :EvaluationReport ;
    rdfs:range rdf:PlainLiteral .

:hasEvaluationJustification rdfs:domain :EvaluationCriterion ;
    rdfs:range rdf:PlainLiteral .

:hasScorePoint rdfs:domain :EvaluationCriterion ;
    rdfs:range rdf:PlainLiteral .

:isAppointedBy a owl:FunctionalProperty ;
    rdfs:domain :EvaluationBoard ;
    rdfs:range :UniqueResponsibleOfTheProcurement .

:providesRankToTender a owl:FunctionalProperty ;
    rdfs:domain :EvaluationReport ;
    rdfs:range :TenderAwardOutcome .

:refersToTenderClarificationRequest a owl:FunctionalProperty ;
    rdfs:domain :TenderClarification ;
    rdfs:range :TenderClarificationRequest .

:reportsEvaluationCriterion rdfs:domain :EvaluationReport ;
    rdfs:range :EvaluationCriterion .

:specifiesEconomicOperator a owl:FunctionalProperty ;
    rdfs:domain :TenderClarificationRequest ;
    rdfs:range :OfferingParty .

:usesEvaluationFormula rdfs:domain :EvaluationReport ;
    rdfs:range :AwardEvaluationTerm .

:writesEvaluationReport rdfs:domain :EvaluationBoard ;
    rdfs:range :EvaluationReport .

cccev:hasRequirement rdfs:domain :TenderClarificationRequest ;
    rdfs:range cccev:InformationRequirement .

:playedBy rdfs:domain [ a owl:Class ;
            owl:unionOf ( :UniqueResponsibleOfTheProcurement :Evaluator ) ] ;
    rdfs:range person:Person .

:Evaluator rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :playedBy ] .

:UniqueResponsibleOfTheProcurement rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :playedBy ] .

:EvaluationCriterion rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasScorePoint ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :fulfillsProcurementCriterion ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :evaluatesProcurementCriterion ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasEvaluationJustification ] .

:EvaluationBoard rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :writesEvaluationReport ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :isAppointedBy ] .

:TenderClarificationRequest rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :concernsEvaluationReport ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty cccev:hasRequirement ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :specifiesEconomicOperator ] .

:EvaluationReport rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty :providesRankToTender ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :hasAwardCriteriaEvaluationScore ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty :usesEvaluationFormula ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty :reportsEvaluationCriterion ] .

[] a owl:AllDisjointClasses ;
    owl:members ( :UniqueResponsibleOfTheProcurement :Evaluator ) .

[] a owl:AllDisjointClasses ;
    owl:members ( :EvaluationReport :TenderClarificationRequest ) .

