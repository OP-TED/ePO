a4g_shape:PlainLiteral a shacl:NodeShape ;
	shacl:or (
		[
			shacl:datatype xsd:string ;
		]
		[
			shacl:datatype rdf:langString ;
		]
	) .

a4g_shape:ESPDRequestShape a shacl:NodeShape ;
	shacl:targetClass a4g:ESPDRequest ;
	rdfs:isDefinedBy a4g_shape:acc-shape ;
	rdfs:comment "An updated self-declaration used by the economic operator as a preliminary evidence in replacement of certificates issued by public authorities or third parties confirming that the economic operator fulfils the Exclusion Grounds and the Selection Criteria set out by the Buyer for a specific Procurement. WG Approval 05/03/2024" ;
	shacl:property a4g_shape:ESPDRequest-specifiesProcurementCriterion-ProcurementCriterion, a4g_shape:ESPDRequest-refersToNotice-Notice, a4g_shape:ESPDRequest-concernsProcedure-Procedure .

a4g_shape:acc-shape a owl:Ontology ;
	rdfs:label "eProcurement Ontology acc - SHACL shapes"@en ;
	dcterms:created "2021-06-01"^^xsd:date ;
	dcterms:description "The eProcurement Ontology acc SHACL shapes provides the datashape specifications for the eProcurement Ontology acc module."@en ;
	dcterms:issued "2024-03-21"^^xsd:date ;
	dcterms:license "Â© European Union, 2014. Unless otherwise noted, the reuse of the Ontology is authorised under the European Union Public Licence v1.2 (https://eupl.eu/)." ;
	dcterms:publisher "http://publications.europa.eu/resource/authority/corporate-body/PUBL" ;
	dcterms:title "eProcurement Ontology acc - SHACL shapes"@en ;
	vann:preferredNamespacePrefix "epo" ;
	vann:preferredNamespaceUri "http://data.europa.eu/a4g/ontology#" ;
	rdfs:seeAlso <https://docs.ted.europa.eu/home/index.html>, <https://github.com/OP-TED/ePO/releases>, <https://joinup.ec.europa.eu/collection/eprocurement/solution/eprocurement-ontology/about>, <https://op.europa.eu/en/web/eu-vocabularies/e-procurement> ;
	owl:imports <http://data.europa.eu/eli/ontology#>, <http://data.europa.eu/m8g/>, <http://purl.org/dc/terms/>, <http://purl.org/vocab/vann/>, <http://www.w3.org/2004/02/skos/core>, <http://www.w3.org/2006/time>, <http://www.w3.org/ns/adms>, <http://www.w3.org/ns/locn>, <http://www.w3.org/ns/org#>, <http://www.w3.org/ns/person>, <http://xmlns.com/foaf/0.1/> ;
	owl:incompatibleWith "3.1.0" ;
	owl:priorVersion "http://data.europa.eu/a4g/data-shape#acc-shape-4.0.2" ;
	owl:versionIRI a4g_shape:acc-shape-4.1.0 ;
	owl:versionInfo "4.1.0" .

a4g_shape:ESPDRequest-specifiesProcurementCriterion-ProcurementCriterion a shacl:PropertyShape ;
	shacl:targetClass a4g:ProcurementCriterion ;
	rdfs:isDefinedBy a4g_shape:acc-shape ;
	shacl:path a4g:specifiesProcurementCriterion ;
	shacl:nodeKind shacl:IRI ;
	shacl:name "specifies procurement criterion"@en ;
	shacl:minCount 1 .

a4g_shape:ESPDRequest-refersToNotice-Notice a shacl:PropertyShape ;
	shacl:targetClass a4g:Notice ;
	rdfs:isDefinedBy a4g_shape:acc-shape ;
	shacl:path a4g:refersToNotice ;
	shacl:nodeKind shacl:IRI ;
	shacl:name "refers to notice"@en ;
	shacl:description "Reference to a Notice. WG approval 12/03/2024" .

a4g_shape:ESPDRequest-concernsProcedure-Procedure a shacl:PropertyShape ;
	shacl:targetClass a4g:Procedure ;
	rdfs:isDefinedBy a4g_shape:acc-shape ;
	shacl:path a4g:concernsProcedure ;
	shacl:nodeKind shacl:IRI ;
	shacl:name "concerns procedure"@en ;
	shacl:minCount 1 ;
	shacl:description "Relates to Procedure. WG approval 05/03/2024 " ;
	shacl:maxCount 1 .

