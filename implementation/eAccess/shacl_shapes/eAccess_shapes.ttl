    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#containsQuestionSubgroup> ?that . ?that <http://data.europa.eu/a4g/ontology#containsQuestionSubgroup> ?this .}" ] .

cat-shape:epo-acc-RequirementGroup-epo-acc-containsQuestionSubgroup a sh:PropertyShape ;
    sh:class :QuestionSubgroup ;
