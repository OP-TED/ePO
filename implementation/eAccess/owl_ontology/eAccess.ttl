:ESPDRequest a owl:Class ;
    rdfs:isDefinedBy :acc ;
    rdfs:subClassOf :ProcurementDocument ;
    skos:definition "An updated self-declaration used by the economic operator as a preliminary evidence in replacement of certificates issued by public authorities or third parties confirming that the economic operator fulfils the Exclusion Grounds and the Selection Criteria set out by the Buyer for a specific Procurement."@en ;
    skos:prefLabel "E s p d request"@en .

:concernsProcedure a owl:ObjectProperty ;
    rdfs:isDefinedBy :acc ;
    skos:definition "Relates to Procedure. (epo-acc:ESPDRequest -> epo:Procedure (+epo:concernsProcedure))"@en ;
    skos:editorialNote "The relations epo-acc:concernsProcedure and epo:refersToProcedure have to refer to the same Procedure."@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Concerns procedure"@en .

:hasResourceType a owl:ObjectProperty ;
    rdfs:isDefinedBy :acc ;
    skos:prefLabel "Has resource type"@en .

:hasSubdivision a owl:ObjectProperty ;
    rdfs:isDefinedBy :acc ;
    skos:prefLabel "Has subdivision"@en .

:refersToNotice a owl:ObjectProperty ;
    rdfs:isDefinedBy :acc ;
    skos:definition "Reference to a Notice. (epo-acc:ESPDRequest -> epo:Notice (+epo-acc:refersToNotice))"@en ;
    skos:historyNote "WG approval 12/03/2024"@en ;
    skos:prefLabel "Refers to notice"@en .

:relatesToESPDRequest a owl:ObjectProperty ;
    rdfs:isDefinedBy :acc ;
    skos:definition "Is about an European Single Procurement Document (ESPD) Request. (epo-sub:ESPD -> epo-acc:ESPDRequest (+epo-sub:relatesToESPDRequest))"@en ;
    skos:historyNote "WG Approval 05/03/2024"@en ;
    skos:prefLabel "Relates to e s p d request"@en .

:specifiesProcurementCriterion a owl:ObjectProperty ;
    rdfs:isDefinedBy :acc ;
    skos:prefLabel "Specifies procurement criterion"@en .

