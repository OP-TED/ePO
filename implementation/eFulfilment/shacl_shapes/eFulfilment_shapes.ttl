@prefix : <http://data.europa.eu/a4g/ontology#> .
@prefix at-voc-new: <http://publications.europa.eu/resource/authority/new/> .
@prefix cccev: <http://data.europa.eu/m8g/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/> .
@prefix ful-shape: <http://data.europa.eu/a4g/data-shape#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ful-shape:epo-ful-AbstractContainer a sh:NodeShape ;
    rdfs:label "Abstract container" ;
    rdfs:comment "Gathering class for Packages and Transport Handling Units." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-AbstractContainer-epo-ful-hasTemperatureSpecification,
        ful-shape:epo-ful-AbstractContainer-epo-ful-isHazardousRisk,
        ful-shape:epo-ful-AbstractContainer-epo-ful-isReturnableMaterial ;
    sh:targetClass :AbstractContainer .

ful-shape:epo-ful-Carrier a sh:NodeShape ;
    rdfs:label "Carrier" ;
    rdfs:comment "The Role of an Agent who handles the physical Delivery/Transportation of the (Despatched) Shipment. Additional Information: This Role is often referred to as the deliverer." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :Carrier .

ful-shape:epo-ful-Consignment a sh:NodeShape ;
    rdfs:label "Consignment" ;
    rdfs:comment "A batch of goods destined for or delivered to someone. Additional information: Consignment is a Transport Agreement between the Despatcher and the Consignee as to who and how the goods are despatched and received." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-Consignment-epo-ful-containsTransportHandlingUnit,
        ful-shape:epo-ful-Consignment-epo-ful-hasCarrierServiceInstruction,
        ful-shape:epo-ful-Consignment-epo-ful-hasDeliveryInstruction,
        ful-shape:epo-ful-Consignment-epo-ful-hasHandlingInstruction,
        ful-shape:epo-ful-Consignment-epo-ful-hasHandlingInstructionCode,
        ful-shape:epo-ful-Consignment-epo-ful-hasMainCarriageShipmentStage,
        ful-shape:epo-ful-Consignment-epo-ful-hasOnCarriageShipmentStage,
        ful-shape:epo-ful-Consignment-epo-ful-hasPreCarriageShipmentStage,
        ful-shape:epo-ful-Consignment-epo-ful-hasRequestedPickUpInformation,
        ful-shape:epo-ful-Consignment-epo-ful-hasSpecialInstruction,
        ful-shape:epo-ful-Consignment-epo-ful-hasSpecialServiceInstruction,
        ful-shape:epo-ful-Consignment-epo-ful-hasVehicleDescription,
        ful-shape:epo-ful-Consignment-epo-ful-isHazardousRisk,
        ful-shape:epo-ful-Consignment-epo-specifiesFreightForwarder,
        ful-shape:epo-ful-Consignment-epo-specifiesNotificationReceiver,
        ful-shape:epo-ful-Consignment-epo-specifiesNotifier ;
    sh:targetClass :Consignment .

ful-shape:epo-ful-DespatchAdvice a sh:NodeShape ;
    rdfs:label "Despatch advice" ;
    rdfs:comment "A document notifying the sending of goods or the execution of works or services." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-DespatchAdvice-epo-ful-comprisesDespatchLine,
        ful-shape:epo-ful-DespatchAdvice-epo-ful-hasDespatchAdviceType,
        ful-shape:epo-ful-DespatchAdvice-epo-ful-refersToConsignment,
        ful-shape:epo-ful-DespatchAdvice-epo-ful-refersToShipmentAgreement,
        ful-shape:epo-ful-DespatchAdvice-epo-ful-specifiesDespatcher,
        ful-shape:epo-ful-DespatchAdvice-epo-ful-specifiesShipment,
        ful-shape:epo-ful-DespatchAdvice-epo-ful-specifiesTransportHandlingUnits ;
    sh:targetClass :DespatchAdvice .

ful-shape:epo-ful-DespatchLine a sh:NodeShape ;
    rdfs:label "Despatch line" ;
    rdfs:comment "Details concerning the fulfilment of an Order Line." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-DespatchLine-epo-ful-hasOutstandingQuantityReason ;
    sh:targetClass :DespatchLine .

ful-shape:epo-ful-Despatcher a sh:NodeShape ;
    rdfs:label "Despatcher" ;
    rdfs:comment "The Role of an Agent who sends the goods or notifies of the service or works execution. Additional Information: The Role is carried out by the Supplier or on behalf of the Supplier. This Role is often referred to as the Despatch Party, Consignor or Deliverer." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :Despatcher .

ful-shape:epo-ful-FreightForwarder a sh:NodeShape ;
    rdfs:label "Freight forwarder" ;
    rdfs:comment "The Role of an Agent who organise the logistic operations need for a fulfilment." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :FreightForwarder .

ful-shape:epo-ful-GoodsItem a sh:NodeShape ;
    rdfs:label "Goods item" ;
    rdfs:comment "An Item including its initial packaging during transportation. Additional information: For example, the Item being bought is the computer. The weight of the computer is X. The weight of the Goods Item is the weight of the computer plus the weight of the initial packaging. During transportation you have for example trace ID which is applied to the GoodsItem, rather than on the Item itself. The declared statistical value can only apply once you have a GoodsItem." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-GoodsItem-epo-ful-refersToDespatchLine ;
    sh:targetClass :GoodsItem .

ful-shape:epo-ful-NotificationReceiver a sh:NodeShape ;
    rdfs:label "Notification receiver" ;
    rdfs:comment "The Role of an Agent that is informed about the arrival of a Shipment at a destination. Additional information: This could be for example customs and excise." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :NotificationReceiver .

ful-shape:epo-ful-Notifier a sh:NodeShape ;
    rdfs:label "Notifier" ;
    rdfs:comment "This class was wrongly named and has been replaced by epo-ful:NotificationReceiver. It will be removed in the next major release." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :Notifier .

ful-shape:epo-ful-Package a sh:NodeShape ;
    rdfs:label "Package" ;
    rdfs:comment "Container used for transportation." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-Package-epo-ful-containsGoodsItem,
        ful-shape:epo-ful-Package-epo-ful-containsPackage,
        ful-shape:epo-ful-Package-epo-ful-hasPackagingType,
        ful-shape:epo-ful-Package-epo-ful-hasPackagingTypeDescription ;
    sh:targetClass :Package .

ful-shape:epo-ful-ReceiptAdvice a sh:NodeShape ;
    rdfs:label "Receipt advice" ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-ReceiptAdvice-epo-ful-comprisesReceiptAdviceLine,
        ful-shape:epo-ful-ReceiptAdvice-epo-ful-isSubmittedForDespatchAdvice,
        ful-shape:epo-ful-ReceiptAdvice-epo-ful-isSubmittedForShipment,
        ful-shape:epo-ful-ReceiptAdvice-epo-ful-specifiesDespatcher ;
    sh:targetClass :ReceiptAdvice .

ful-shape:epo-ful-ReceiptAdviceLine a sh:NodeShape ;
    rdfs:label "Receipt advice line" ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-ReceiptAdviceLine-epo-ful-RejectionAction,
        ful-shape:epo-ful-ReceiptAdviceLine-epo-ful-hasRejectReason,
        ful-shape:epo-ful-ReceiptAdviceLine-epo-ful-hasShortageAction,
        ful-shape:epo-ful-ReceiptAdviceLine-epo-ful-isSubmittedForDespatchLine ;
    sh:targetClass :ReceiptAdviceLine .

ful-shape:epo-ful-ShipmentAgreement a sh:NodeShape ;
    rdfs:label "Shipment agreement" ;
    rdfs:comment "The commercial agreement between the Buyer and the Seller. Additional Information: The Shipment Agreement describes the items being shipped." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :ShipmentAgreement .

ful-shape:epo-ful-ShipmentInformation a sh:NodeShape ;
    rdfs:label "Shipment information" ;
    rdfs:comment "Information about the transportation of an identifiable collection of Goods Items from the Despatcher to the Consignee." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-ShipmentInformation-dct-description,
        ful-shape:epo-ful-ShipmentInformation-epo-ful-despatchDate,
        ful-shape:epo-ful-ShipmentInformation-epo-ful-isSpecificToDespatchLine ;
    sh:targetClass :ShipmentInformation .

ful-shape:epo-ful-ShipmentStage a sh:NodeShape ;
    rdfs:label "Shipment stage" ;
    rdfs:comment "Part of the itinerary of a shipment. Additional information: In the context of fulfilment only the last part of the itinerary is taken into consideration." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-ShipmentStage-epo-ful-hasDepartureShipmentInformation,
        ful-shape:epo-ful-ShipmentStage-epo-ful-isHazardousRisk,
        ful-shape:epo-ful-ShipmentStage-epo-ful-usesTransportMeans,
        ful-shape:epo-ful-ShipmentStage-epo-hasArrivalShipmentInformation,
        ful-shape:epo-ful-ShipmentStage-epo-specifiesCarrier ;
    sh:targetClass :ShipmentStage .

ful-shape:epo-ful-TemperatureSpecification a sh:NodeShape ;
    rdfs:label "Temperature specification" ;
    rdfs:comment "Description of the temperature requirements during shipment." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-TemperatureSpecification-dct-description ;
    sh:targetClass :TemperatureSpecification .

ful-shape:epo-ful-TransportEquipment a sh:NodeShape ;
    rdfs:label "Transport equipment" ;
    rdfs:comment "Description of the apparatus used during transportation. Additional information: The equipment may be a container, a trailer, means of assistance to load or unload the Goods Items." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-TransportEquipment-dct-description,
        ful-shape:epo-ful-TransportEquipment-epo-ful-hasSizeType,
        ful-shape:epo-ful-TransportEquipment-epo-ful-hasTransportEquipmentSeal,
        ful-shape:epo-ful-TransportEquipment-epo-ful-hasTransportEquipmentType,
        ful-shape:epo-ful-TransportEquipment-epo-ful-isPowerIndicated,
        ful-shape:epo-ful-TransportEquipment-epo-ful-isRefrigerated,
        ful-shape:epo-ful-TransportEquipment-epo-ful-isReturnable ;
    sh:targetClass :TransportEquipment .

ful-shape:epo-ful-TransportEquipmentSeal a sh:NodeShape ;
    rdfs:label "Transport equipment seal" ;
    rdfs:comment "Means used to ensure that the Transport Equipment is not tampered during transportation." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :TransportEquipmentSeal .

ful-shape:epo-ful-TransportHandlingUnit a sh:NodeShape ;
    rdfs:label "Transport handling unit" ;
    rdfs:comment "The outermost Package that the Carrier is handling." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-TransportHandlingUnit-epo-ful-hasHandlingInstruction,
        ful-shape:epo-ful-TransportHandlingUnit-epo-ful-hasHandlingInstructionCode,
        ful-shape:epo-ful-TransportHandlingUnit-epo-ful-hasShippingMark,
        ful-shape:epo-ful-TransportHandlingUnit-epo-ful-transportsItemFrom,
        ful-shape:epo-ful-TransportHandlingUnit-epo-ful-usesTransportEquipment ;
    sh:targetClass :TransportHandlingUnit .

ful-shape:epo-ful-TransportMeans a sh:NodeShape ;
    rdfs:label "Transport means" ;
    rdfs:comment "Description of a particular vehicle or vessel used for the conveyance of goods or persons." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:property ful-shape:epo-ful-TransportMeans-epo-ful-hasTransportMeansType,
        ful-shape:epo-ful-TransportMeans-epo-ful-hasTransportMode,
        ful-shape:epo-ful-TransportMeans-epo-hasDriverPerson ;
    sh:targetClass :TransportMeans .

ful-shape:epo-ful-TransportMeansOperator a sh:NodeShape ;
    rdfs:label "Transport means operator" ;
    rdfs:comment "The Role of an Agent who is responsible for controlling the Transport Means." ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:targetClass :TransportMeansOperator .

ful-shape:epo-ful-AbstractContainer-epo-ful-hasTemperatureSpecification a sh:PropertyShape ;
    sh:class :TemperatureSpecification ;
    sh:description "Relation indicating the Temperature Specification for the instance of the concept." ;
    sh:maxCount 1 ;
    sh:name "Has temperature specification" ;
    sh:path :hasTemperatureSpecification ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasTemperatureSpecification> ?that . ?that <http://data.europa.eu/a4g/ontology#hasTemperatureSpecification> ?this .}" ] .

ful-shape:epo-ful-AbstractContainer-epo-ful-isHazardousRisk a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype xsd:boolean ;
    sh:description "Indicates whether some of the contained Goods Items are hazardous." ;
    sh:maxCount 1 ;
    sh:name "Is hazardous risk" ;
    sh:path :isHazardousRisk .

ful-shape:epo-ful-AbstractContainer-epo-ful-isReturnableMaterial a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype xsd:boolean ;
    sh:description "Indicates whether the container or part of the container is to be sent back." ;
    sh:maxCount 1 ;
    sh:name "Is returnable material" ;
    sh:path :isReturnableMaterial .

ful-shape:epo-ful-Consignment-epo-ful-containsTransportHandlingUnit a sh:PropertyShape ;
    sh:class :TransportHandlingUnit ;
    sh:description "Includes Transport Handling Unit." ;
    sh:minCount 1 ;
    sh:name "Contains transport handling unit" ;
    sh:path :containsTransportHandlingUnit ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#containsTransportHandlingUnit> ?that . ?that <http://data.europa.eu/a4g/ontology#containsTransportHandlingUnit> ?this .}" ] .

ful-shape:epo-ful-Consignment-epo-ful-hasCarrierServiceInstruction a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "This attribute should be removed in the next major release." ;
    sh:name "Has carrier service instruction" ;
    sh:path :hasCarrierServiceInstruction .

ful-shape:epo-ful-Consignment-epo-ful-hasDeliveryInstruction a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "This attribute should be removed in the next major release." ;
    sh:name "Has delivery instruction" ;
    sh:path :hasDeliveryInstruction .

ful-shape:epo-ful-Consignment-epo-ful-hasHandlingInstruction a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "Instruction on how to handle the Goods Items during transportation." ;
    sh:name "Has handling instruction" ;
    sh:path :hasHandlingInstruction .

ful-shape:epo-ful-Consignment-epo-ful-hasHandlingInstructionCode a sh:PropertyShape ;
    sh:class at-voc-new:handling-instruction ;
    sh:description "Instruction on how to handle the Goods Items during transportation, expressed as a code." ;
    sh:maxCount 1 ;
    sh:name "Has handling instruction code" ;
    sh:path :hasHandlingInstructionCode .

ful-shape:epo-ful-Consignment-epo-ful-hasMainCarriageShipmentStage a sh:PropertyShape ;
    sh:class :ShipmentStage ;
    sh:description "The Shipment Stage for the longest distance covered in a transport chain." ;
    sh:maxCount 1 ;
    sh:name "Has main carriage shipment stage" ;
    sh:path :hasMainCarriageShipmentStage ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasMainCarriageShipmentStage> ?that . ?that <http://data.europa.eu/a4g/ontology#hasMainCarriageShipmentStage> ?this .}" ] .

ful-shape:epo-ful-Consignment-epo-ful-hasOnCarriageShipmentStage a sh:PropertyShape ;
    sh:class :ShipmentStage ;
    sh:description "Any Shipment Stage between the main carriage arrival point and the ultimate receiver in a transport chain." ;
    sh:name "Has on carriage shipment stage" ;
    sh:path :hasOnCarriageShipmentStage ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasOnCarriageShipmentStage> ?that . ?that <http://data.europa.eu/a4g/ontology#hasOnCarriageShipmentStage> ?this .}" ] .

ful-shape:epo-ful-Consignment-epo-ful-hasPreCarriageShipmentStage a sh:PropertyShape ;
    sh:class :ShipmentStage ;
    sh:description "Any Shipment Stage between the Despatcher and the departure point of the main carriage in a transport chain." ;
    sh:name "Has pre carriage shipment stage" ;
    sh:path :hasPreCarriageShipmentStage ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasPreCarriageShipmentStage> ?that . ?that <http://data.europa.eu/a4g/ontology#hasPreCarriageShipmentStage> ?this .}" ] .

ful-shape:epo-ful-Consignment-epo-ful-hasRequestedPickUpInformation a sh:PropertyShape ;
    sh:class :ShipmentInformation ;
    sh:description "Information about the requested despatch of the Goods Items." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Has requested pick up information" ;
    sh:path :hasRequestedPickUpInformation ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#hasRequestedPickUpInformation> ?that . ?that <http://data.europa.eu/a4g/ontology#hasRequestedPickUpInformation> ?this .}" ] .

ful-shape:epo-ful-Consignment-epo-ful-hasSpecialInstruction a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "This attribute should be removed in the next major release." ;
    sh:name "Has special instruction" ;
    sh:path :hasSpecialInstruction .

ful-shape:epo-ful-Consignment-epo-ful-hasSpecialServiceInstruction a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "Description about special services needed for loading or unloading the Goods Items." ;
    sh:name "Has special service instruction" ;
    sh:path :hasSpecialServiceInstruction .

ful-shape:epo-ful-Consignment-epo-ful-hasVehicleDescription a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype rdf:PlainLiteral ;
    sh:description "" ;
    sh:maxCount 1 ;
    sh:name "Has vehicle description" ;
    sh:path :hasVehicleDescription .

ful-shape:epo-ful-Consignment-epo-ful-isHazardousRisk a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype xsd:boolean ;
    sh:description "Indicates whether some of the contained Goods Items are hazardous." ;
    sh:maxCount 1 ;
    sh:name "Is hazardous risk" ;
    sh:path :isHazardousRisk .

ful-shape:epo-ful-Consignment-epo-specifiesFreightForwarder a sh:PropertyShape ;
    sh:class :FreightForwarder ;
    sh:description "Relation indicating that the instance of the concept involves the Freight Forwarder." ;
    sh:name "Specifies freight forwarder" ;
    sh:path :specifiesFreightForwarder ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#specifiesFreightForwarder> ?that . ?that <http://data.europa.eu/a4g/ontology#specifiesFreightForwarder> ?this .}" ] .

ful-shape:epo-ful-Consignment-epo-specifiesNotificationReceiver a sh:PropertyShape ;
