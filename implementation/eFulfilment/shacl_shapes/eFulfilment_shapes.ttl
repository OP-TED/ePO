    sh:description "c" ;
    sh:name "Specifies notification receiver" ;
    sh:path :specifiesNotificationReceiver ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#specifiesNotificationReceiver> ?that . ?that <http://data.europa.eu/a4g/ontology#specifiesNotificationReceiver> ?this .}" ] .

ful-shape:epo-ful-Consignment-epo-specifiesNotifier a sh:PropertyShape ;
    sh:class :Notifier ;
    sh:name "Specifies notifier" ;
    sh:path :specifiesNotifier ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#specifiesNotifier> ?that . ?that <http://data.europa.eu/a4g/ontology#specifiesNotifier> ?this .}" ] .

ful-shape:epo-ful-DespatchAdvice-epo-ful-comprisesDespatchLine a sh:PropertyShape ;
    sh:class :DespatchLine ;
    sh:description "Incorporates Despatch Line." ;
    sh:minCount 1 ;
    sh:name "Comprises despatch line" ;
    sh:path :comprisesDespatchLine ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#comprisesDespatchLine> ?that . ?that <http://data.europa.eu/a4g/ontology#comprisesDespatchLine> ?this .}" ] .

ful-shape:epo-ful-DespatchAdvice-epo-ful-hasDespatchAdviceType a sh:PropertyShape ;
    sh:class at-voc-new:despatch-advice-type ;
    sh:description "Classification of a Despatch Advice." ;
    sh:maxCount 1 ;
    sh:name "Has despatch advice type" ;
    sh:path :hasDespatchAdviceType .

ful-shape:epo-ful-DespatchAdvice-epo-ful-refersToConsignment a sh:PropertyShape ;
    sh:class :Consignment ;
    sh:description "Reference to a Consignment." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Refers to consignment" ;
    sh:path :refersToConsignment ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#refersToConsignment> ?that . ?that <http://data.europa.eu/a4g/ontology#refersToConsignment> ?this .}" ] .

ful-shape:epo-ful-DespatchAdvice-epo-ful-refersToShipmentAgreement a sh:PropertyShape ;
    sh:class :ShipmentAgreement ;
    sh:description "Reference to a Shipment Agreement." ;
    sh:maxCount 1 ;
    sh:name "Refers to shipment agreement" ;
    sh:path :refersToShipmentAgreement ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#refersToShipmentAgreement> ?that . ?that <http://data.europa.eu/a4g/ontology#refersToShipmentAgreement> ?this .}" ] .

ful-shape:epo-ful-DespatchAdvice-epo-ful-specifiesDespatcher a sh:PropertyShape ;
    sh:class :Despatcher ;
    sh:description "Relation indicating that the instance of the concept involves the Despatcher." ;
    sh:minCount 1 ;
    sh:name "Specifies despatcher" ;
    sh:path :specifiesDespatcher ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#specifiesDespatcher> ?that . ?that <http://data.europa.eu/a4g/ontology#specifiesDespatcher> ?this .}" ] .

ful-shape:epo-ful-DespatchAdvice-epo-ful-specifiesShipment a sh:PropertyShape ;
    sh:class :ShipmentInformation ;
    sh:maxCount 1 ;
    sh:name "Specifies shipment" ;
    sh:path :specifiesShipment ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#specifiesShipment> ?that . ?that <http://data.europa.eu/a4g/ontology#specifiesShipment> ?this .}" ] .

ful-shape:epo-ful-DespatchAdvice-epo-ful-specifiesTransportHandlingUnits a sh:PropertyShape ;
    sh:class :TransportHandlingUnit ;
    sh:description "Relation indicating the use of a Transport Handling Unit." ;
    sh:name "Specifies transport handling units" ;
    sh:path :specifiesTransportHandlingUnits ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/a4g/ontology#specifiesTransportHandlingUnits> ?that . ?that <http://data.europa.eu/a4g/ontology#specifiesTransportHandlingUnits> ?this .}" ] .

ful-shape:epo-ful-DespatchLine-epo-ful-hasOutstandingQuantityReason a sh:PropertyShape ;
    rdfs:isDefinedBy ful-shape:ful-shape ;
    sh:datatype rdf:PlainLiteral ;
