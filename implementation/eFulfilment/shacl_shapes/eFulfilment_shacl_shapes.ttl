epo:Consignment a sh:NodeShape ;
    rdfs:label "Consignment"@en ;
    rdfs:comment """A batch of goods destined for or delivered to someone.

Additional information:
Consignment it is a Transport Agreement. They do not care about the content of what is being shipped. (only if there are dangerous goods, and temperature requirements). How to move from place a to place B."""@en ;
    rdfs:isDefinedBy <http://data.europa.eu/a4g/ontology> ;
    sh:description """A batch of goods destined for or delivered to someone.

Additional information:
Consignment it is a Transport Agreement. They do not care about the content of what is being shipped. (only if there are dangerous goods, and temperature requirements). How to move from place a to place B.""" ;
    sh:name "Consignment" ;
    sh:property [ a sh:PropertyShape ;
            sh:class epo:ShipmentStage ;
            sh:name "Has on carriage shipment stage" ;
            sh:path epo:hasOnCarriageShipmentStage ],
        [ a sh:PropertyShape ;
            sh:datatype xsd:boolean ;
            sh:name "Is hazardous risk" ;
            sh:path epo:isHazardousRisk ],
        [ a sh:PropertyShape ;
            sh:class epo:Notifier ;
            sh:name "Specifies notifier" ;
            sh:path epo:specifiesNotifier ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has vehicle description" ;
            sh:path epo:hasVehicleDescription ],
        [ a sh:PropertyShape ;
            sh:class epo:TransportHandlingUnit ;
            sh:name "Contains transport handling unit" ;
            sh:path epo:containsTransportHandlingUnit ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Is hazardous risk" ;
            sh:path epo:isHazardousRisk ],
        [ a sh:PropertyShape ;
            sh:class epo:ShipmentInformation ;
            sh:name "Has requested pick up information" ;
            sh:path epo:hasRequestedPickUpInformation ],
        [ a sh:PropertyShape ;
            sh:class epo:ShipmentStage ;
            sh:name "Has main carriage shipment stage" ;
            sh:path epo:hasMainCarriageShipmentStage ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has main carriage shipment stage" ;
            sh:path epo:hasMainCarriageShipmentStage ],
        [ a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:name "Contains transport handling unit" ;
            sh:path epo:containsTransportHandlingUnit ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "Has requested pick up information" ;
            sh:path epo:hasRequestedPickUpInformation ],
        [ a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "Has handling instruction" ;
            sh:path epo:hasHandlingInstruction ],
        [ a sh:PropertyShape ;
            sh:class epo:FreightForwarder ;
            sh:name "Specifies freight forwarder" ;
