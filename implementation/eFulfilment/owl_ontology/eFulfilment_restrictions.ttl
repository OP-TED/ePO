epo:agreedByBuyer rdfs:domain epo:ShipmentAgreement ;
    rdfs:range epo:Buyer .

epo:agreedBySeller rdfs:domain epo:ShipmentAgreement ;
    rdfs:range epo:Seller .

epo:containsPackage a owl:AsymmetricProperty ;
    rdfs:domain epo:Package ;
    rdfs:range epo:Package .

epo:hasAllowanceChargeReason rdfs:domain epo:AllowanceChargeInformation ;
    rdfs:range <skos:Concept> .

epo:hasArrivalShipmentInformation rdfs:domain epo:ShipmentStage ;
    rdfs:range epo:ShipmentInformation .

epo:hasAssociatedDocument rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:DespatchAdvice epo:DespatchLine ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:Document ) ] .

epo:hasCarrierConsignmentID rdfs:domain epo:Consignment ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasCarrierServiceInstruction rdfs:domain epo:Consignment ;
    rdfs:range rdf:PlainLiteral .

epo:hasChargeableWeight rdfs:domain epo:Consignment ;
    rdfs:range epo:Quantity .

epo:hasConsigneeConsignmentID rdfs:domain epo:Consignment ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasConsignmentDeclaredStatisticsValue rdfs:domain epo:Consignment ;
    rdfs:range epo:MonetaryValue .

epo:hasConsignmentFreeOnBoardValue rdfs:domain epo:Consignment ;
    rdfs:range epo:MonetaryValue .

epo:hasConsignmentInvoiceValue rdfs:domain epo:Consignment ;
    rdfs:range epo:MonetaryValue .

epo:hasDeclaredStatisticalValue rdfs:domain epo:GoodsItem ;
    rdfs:range epo:MonetaryValue .

epo:hasDeliveryInstruction rdfs:domain epo:Consignment ;
    rdfs:range rdf:PlainLiteral .

epo:hasDepartureShipmentInformation rdfs:domain epo:ShipmentStage ;
    rdfs:range epo:ShipmentInformation .

epo:hasDespatchAdviceType rdfs:domain epo:DespatchAdvice ;
    rdfs:range <skos:Concept> .

epo:hasDespatchedQuantity rdfs:domain epo:DespatchLine ;
    rdfs:range epo:Quantity .

epo:hasDocumentStatus rdfs:domain epo:Document ;
    rdfs:range <skos:Concept> .

epo:hasEstimatedDeliveryPeriod rdfs:domain epo:ShipmentInformation ;
    rdfs:range epo:Period .

epo:hasFreightAllowanceCharge rdfs:domain epo:Consignment ;
    rdfs:range epo:ChargeInformation .

epo:hasFreightForwarderConsignmentID rdfs:domain epo:Consignment ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasGrossVolume rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AbstractContainer epo:Consignment ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:Quantity ) ] .

epo:hasGrossWeight rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AbstractContainer epo:Consignment ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:Quantity ) ] .

epo:hasHandlingInstruction rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:Consignment epo:TransportHandlingUnit ) ],
        [ a owl:Class ;
            owl:unionOf ( epo:Consignment epo:TransportHandlingUnit ) ] ;
    rdfs:range rdf:PlainLiteral,
        <skos:Concept> .

epo:hasHeight rdfs:domain epo:AbstractContainer ;
    rdfs:range epo:Quantity .

epo:hasLength rdfs:domain epo:AbstractContainer ;
    rdfs:range epo:Quantity .

epo:hasLoadingLength rdfs:domain epo:Consignment ;
    rdfs:range epo:Quantity .

epo:hasMaximumTemperature rdfs:domain epo:TemperatureSpecification ;
    rdfs:range epo:Quantity .

epo:hasMinimumTemperature rdfs:domain epo:TemperatureSpecification ;
    rdfs:range epo:Quantity .

epo:hasNetVolume rdfs:domain epo:AbstractContainer ;
    rdfs:range epo:Quantity .

epo:hasNetWeight rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AbstractContainer epo:Consignment ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:Quantity ) ] .

epo:hasOnCarriageShipmentStage rdfs:domain epo:Consignment ;
    rdfs:range epo:ShipmentStage .

epo:hasOperatorLicenceID rdfs:domain epo:TransportMeansOperator ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasOutstandingQuantity rdfs:domain epo:DespatchLine ;
    rdfs:range epo:Quantity .

epo:hasPackagingType rdfs:domain epo:Package ;
    rdfs:range <skos:Concept> .

epo:hasPreCarriageShipmentStage rdfs:domain epo:Consignment ;
    rdfs:range epo:ShipmentStage .

epo:hasShippingMark rdfs:domain epo:TransportHandlingUnit ;
    rdfs:range rdf:PlainLiteral .

epo:hasSizeType rdfs:domain epo:TransportEquipment ;
    rdfs:range <skos:Concept> .

epo:hasSpecialInstruction rdfs:domain epo:Consignment ;
    rdfs:range rdf:PlainLiteral .

epo:hasSpecialServiceInstruction rdfs:domain epo:Consignment ;
    rdfs:range rdf:PlainLiteral .

epo:hasTotalGoodsItemQuantity rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AbstractContainer epo:Consignment ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( epo:Quantity ) ] .

epo:hasTraceID rdfs:domain epo:GoodsItem ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasTransportEquipmentSeal rdfs:domain epo:TransportEquipment ;
    rdfs:range epo:TransportEquipmentSeal .

epo:hasTransportEquipmentType rdfs:domain epo:TransportEquipment ;
    rdfs:range <skos:Concept> .

epo:hasTransportHandlingUnitQuantity rdfs:domain epo:Consignment ;
    rdfs:range epo:Quantity .

epo:hasTransportMeansType rdfs:domain epo:TransportMeans ;
    rdfs:range <skos:Concept> .

epo:hasTransportMode rdfs:domain epo:TransportMeans ;
    rdfs:range <skos:Concept> .

epo:hasVehicleID rdfs:domain epo:TransportMeans ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasVehicleSegmentID rdfs:domain epo:TransportMeans ;
    rdfs:range <http://unknown.domain/for/prefix#Identifier> .

epo:hasWidth rdfs:domain epo:AbstractContainer ;
    rdfs:range epo:Quantity .

epo:identifier rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AbstractContainer epo:Consignment epo:ShipmentInformation epo:ShipmentStage epo:TransportEquipment epo:TransportEquipmentSeal ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( <http://unknown.domain/for/prefix#Identifier> ) ] .

epo:playedBy rdfs:domain epo:TransportMeansOperator ;
    rdfs:range foaf:Person .

epo:refersToDeliveryInformation rdfs:domain epo:ShipmentInformation ;
    rdfs:range epo:DeliveryInformation .

epo:refersToOrder rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:Order .

epo:refersToOrderLine rdfs:domain epo:DespatchLine ;
    rdfs:range epo:OrderLine .

epo:specifiesConsignee rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:Consignee .

epo:specifiesFreightForwarder rdfs:domain epo:Consignment ;
    rdfs:range epo:FreightForwarder .

epo:specifiesNotifier rdfs:domain epo:Consignment ;
    rdfs:range epo:Notifier .

epo:specifiesOriginator rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:Originator .

epo:specifiesPlaceOfDespatch rdfs:domain epo:ShipmentInformation ;
    rdfs:range dcterms:Location .

epo:specifiesTransportHandlingUnits rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:TransportHandlingUnit .

epo:usesTransportEquipment rdfs:domain epo:TransportHandlingUnit ;
    rdfs:range epo:TransportEquipment .

<http://data.europa.eu/a4g/rule> a owl:Ontology ;
    rdfs:label """eProcurement Ontology - eFulfilment. This module provides the inference-related definitions.
            """@en ;
    cc:attributionName "PublicationsOffice of the European Union" ;
