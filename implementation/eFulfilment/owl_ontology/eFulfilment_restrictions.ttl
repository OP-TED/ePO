epo:DespatchAdviceDocument a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:announcesDespatchAdvice ] .

epo:announcesDespatchAdvice rdfs:domain epo:DespatchAdviceDocument ;
    rdfs:range epo:DespatchAdvice .

epo:comprisesDespatchLine rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:DespatchLine .

epo:containsGoodsItem rdfs:domain epo:Package ;
    rdfs:range epo:GoodsItem .

epo:containsTransportHandlingUnit rdfs:domain epo:Consignment ;
    rdfs:range epo:TransportHandlingUnit .

epo:despatchDate rdfs:domain epo:ShipmentInformation ;
    rdfs:range xsd:DateTime .

epo:hasDriverPerson rdfs:domain epo:TransportMeans ;
    rdfs:range epo:TransportMeansOperator .

epo:hasMainCarriageShipmentStage rdfs:domain epo:Consignment ;
    rdfs:range epo:ShipmentStage .

epo:hasOutstandingQuantityReason rdfs:domain epo:DespatchLine ;
    rdfs:range rdf:PlainLiteral .

epo:hasPackagingTypeDescription rdfs:domain epo:Package ;
    rdfs:range rdf:PlainLiteral .

epo:hasRequestedPickUpInformation a owl:FunctionalProperty ;
    rdfs:domain epo:Consignment ;
    rdfs:range epo:ShipmentInformation .

epo:hasTemperatureSpecification rdfs:domain epo:AbstractContainer ;
    rdfs:range epo:TemperatureSpecification .

epo:hasVehicleDescription rdfs:domain epo:Consignment ;
    rdfs:range rdf:PlainLiteral .

epo:isPowerIndicated rdfs:domain epo:TransportEquipment ;
    rdfs:range xsd:boolean .

epo:isRefrigerated rdfs:domain epo:TransportEquipment ;
    rdfs:range xsd:boolean .

epo:isReturnable rdfs:domain epo:TransportEquipment ;
    rdfs:range xsd:boolean .

epo:isReturnableMaterial rdfs:domain epo:AbstractContainer ;
    rdfs:range xsd:boolean .

epo:isSpecificToDespatchLine rdfs:domain epo:ShipmentInformation ;
    rdfs:range epo:DespatchLine .

epo:refersToConsignment a owl:FunctionalProperty ;
    rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:Consignment .

epo:refersToDespatchLine a owl:FunctionalProperty ;
    rdfs:domain epo:GoodsItem ;
    rdfs:range epo:DespatchLine .

epo:refersToShipmentAgreement rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:ShipmentAgreement .

epo:specifiesCarrier rdfs:domain epo:ShipmentStage ;
    rdfs:range epo:Carrier .

epo:specifiesDespatcher rdfs:domain epo:DespatchAdvice,
        epo:Order ;
    rdfs:range epo:Despatcher .

epo:specifiesShipment rdfs:domain epo:DespatchAdvice ;
    rdfs:range epo:ShipmentInformation .

epo:transportsItemFrom rdfs:domain epo:TransportHandlingUnit ;
    rdfs:range epo:DespatchLine .

epo:usesTransportMeans rdfs:domain epo:ShipmentStage ;
    rdfs:range epo:TransportMeans .

dcterms:description rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:ShipmentInformation epo:TemperatureSpecification epo:TransportEquipment ) ] ;
    rdfs:range rdf:PlainLiteral .

epo:isHazardousRisk rdfs:domain [ a owl:Class ;
            owl:unionOf ( epo:AbstractContainer epo:Consignment epo:ShipmentStage ) ] ;
    rdfs:range xsd:boolean .

epo:GoodsItem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:refersToDespatchLine ] .

epo:TemperatureSpecification a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty dcterms:description ] .

epo:Package a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasPackagingTypeDescription ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:containsGoodsItem ] .

epo:TransportMeans a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:hasDriverPerson ] .

epo:TransportHandlingUnit a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:transportsItemFrom ] .

epo:DespatchLine a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasOutstandingQuantityReason ] .

epo:ShipmentStage a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:specifiesCarrier ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:usesTransportMeans ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isHazardousRisk ] .

epo:TransportEquipment a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isPowerIndicated ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isReturnable ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isRefrigerated ] .

epo:ShipmentInformation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:despatchDate ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isSpecificToDespatchLine ] .

epo:AbstractContainer a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isReturnableMaterial ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isHazardousRisk ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasTemperatureSpecification ] .

epo:DespatchAdvice a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:refersToShipmentAgreement ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:refersToConsignment ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:specifiesDespatcher ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:specifiesShipment ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:comprisesDespatchLine ] .

epo:Consignment a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasVehicleDescription ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty epo:hasRequestedPickUpInformation ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:isHazardousRisk ],
        [ a owl:Restriction ;
            owl:maxCardinality 1 ;
            owl:onProperty epo:hasMainCarriageShipmentStage ],
        [ a owl:Restriction ;
            owl:minCardinality 1 ;
            owl:onProperty epo:containsTransportHandlingUnit ] .

