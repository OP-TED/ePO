a4g:Consignment a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "A batch of goods destined for or delivered to someone. Additional information: Consignment is a Transport Agreement between the Despatcher and the Consignee as to who and how the goods are despatched and received."@en ;
	skos:prefLabel "Consignment"@en .

a4g:DespatchAdvice a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:PostAwardDocument ;
	skos:definition "A document notifying the sending of goods or the execution of works or services."@en ;
	skos:prefLabel "Despatch advice"@en .

a4g:DespatchLine a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:Line ;
	skos:definition "Details concerning the fulfilment of an Order Line."@en ;
	skos:prefLabel "Despatch line"@en .

a4g:Despatcher a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:OfferingParty ;
	skos:definition "The Role of an Agent who sends the goods or notifies of the service or works execution. Additional Information: The Role is carried out by the Supplier or on behalf of the Supplier. This Role is often referred to as the Despatch Party, Consignor or Deliverer."@en ;
	skos:prefLabel "Despatcher"@en .

a4g:FreightForwarder a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:AuxiliaryParty ;
	skos:definition "The Role of an Agent who organise the logistic operations need for a fulfilment."@en ;
	skos:prefLabel "Freight forwarder"@en .

a4g:GoodsItem a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "An Item including its initial packaging during transportation. Additional information: For example, the Item being bought is the computer. The weight of the computer is X. The weight of the Goods Item is the weight of the computer plus the weight of the initial packaging. During transportation you have for example trace ID which is applied to the GoodsItem, rather than on the Item itself. The declared statistical value can only apply once you have a GoodsItem."@en ;
	skos:prefLabel "Goods item"@en .

a4g:NotificationReceiver a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:AuxiliaryParty ;
	skos:definition "The Role of an Agent that is informed about the arrival of a Shipment at a destination. Additional information: This could be for example customs and excise."@en ;
	skos:prefLabel "Notification receiver"@en .

a4g:Notifier a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:AuxiliaryParty ;
	skos:definition "This class was wrongly named and has been replaced by epo-ful:NotificationReceiver. It will be removed in the next major release."@en ;
	skos:prefLabel "Notifier"@en .

a4g:ReceiptAdvice a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:PostAwardDocument ;
	skos:prefLabel "Receipt advice"@en .

a4g:ReceiptAdviceLine a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:Line ;
	skos:prefLabel "Receipt advice line"@en .

a4g:RejectionAction a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:prefLabel "Rejection action"@en .

a4g:ShipmentAgreement a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The commercial agreement between the Buyer and the Seller. Additional Information: The Shipment Agreement describes the items being shipped."@en ;
	skos:prefLabel "Shipment agreement"@en .

a4g:ShipmentInformation a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:InformationHub ;
	skos:definition "Information about the transportation of an identifiable collection of Goods Items from the Despatcher to the Consignee."@en ;
	skos:prefLabel "Shipment information"@en .

a4g:ShipmentStage a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Part of the itinerary of a shipment. Additional information: In the context of fulfilment only the last part of the itinerary is taken into consideration."@en ;
	skos:prefLabel "Shipment stage"@en .

a4g:TemperatureSpecification a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Description of the temperature requirements during shipment."@en ;
	skos:prefLabel "Temperature specification"@en .

a4g:TransportEquipment a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Description of the apparatus used during transportation. Additional information: The equipment may be a container, a trailer, means of assistance to load or unload the Goods Items."@en ;
	skos:prefLabel "Transport equipment"@en .

a4g:TransportEquipmentSeal a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Means used to ensure that the Transport Equipment is not tampered during transportation."@en ;
	skos:prefLabel "Transport equipment seal"@en .

a4g:TransportHandlingUnit a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:Package ;
	skos:definition "The outermost Package that the Carrier is handling."@en ;
	skos:prefLabel "Transport handling unit"@en .

a4g:Package a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:AbstractContainer ;
	skos:definition "Container used for transportation."@en ;
	skos:prefLabel "Package"@en .

a4g:AbstractContainer a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Gathering class for Packages and Transport Handling Units."@en ;
	skos:prefLabel "Abstract container"@en .

a4g:TransportMeans a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Description of a particular vehicle or vessel used for the conveyance of goods or persons."@en ;
	skos:prefLabel "Transport means"@en .

a4g:TransportMeansOperator a owl:Class ;
	rdfs:isDefinedBy a4g:ful ;
	rdfs:subClassOf a4g:AuxiliaryParty ;
	skos:definition "The Role of an Agent who is responsible for controlling the Transport Means."@en ;
	skos:prefLabel "Transport means operator"@en .

a4g:agreedByBuyer a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Accepted by Buyer. (epo-ful:ShipmentAgreement -> epo:Buyer (+epo-ful:agreedByBuyer))"@en ;
	skos:prefLabel "Agreed by buyer"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:agreedBySeller a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Accepted by Seller. (epo-ful:ShipmentAgreement -> epo-ord:Seller (+epo-ful:agreedBySeller))"@en ;
	skos:prefLabel "Agreed by seller"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:comprisesDespatchLine a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Incorporates Despatch Line. (epo-ful:DespatchAdvice -> epo-ful:DespatchLine (+epo-ful:comprisesDespatchLine))"@en ;
	skos:prefLabel "Comprises despatch line"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:comprisesReceiptAdviceLine a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:prefLabel "Comprises receipt advice line"@en .

a4g:containsGoodsItem a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Includes Goods Item. (epo-ful:Package -> epo-ful:GoodsItem (+epo-ful:containsGoodsItem))"@en ;
	skos:prefLabel "Contains goods item"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:containsPackage a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Includes Package. (epo-ful:Package -> epo-ful:Package (+epo-ful:containsPackage))"@en ;
	skos:prefLabel "Contains package"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:containsTransportHandlingUnit a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Includes Transport Handling Unit. (epo-ful:Consignment -> epo-ful:TransportHandlingUnit (+epo-ful:containsTransportHandlingUnit))"@en ;
	skos:prefLabel "Contains transport handling unit"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:despatchDate a owl:DatatypeProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The date of sending of goods or the execution of works or services. (epo-ful:ShipmentInformation)"@en ;
	skos:prefLabel "Despatch date"@en ;
	skos:historyNote "WG Approval 05/06/2024"@en .

a4g:foreseesPackage a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:prefLabel "Foresees package"@en .

a4g:hasAllowanceChargeReason a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The justification for using the Allowance or the Charge. (epo-ord:AllowanceChargeInformation -> at-voc-new:allowance-charge-reason (+epo-ful:hasAllowanceChargeReason))"@en ;
	skos:prefLabel "Has allowance charge reason"@en .

a4g:hasArrivalShipmentInformation a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Shipment Information about the destination of the Consignment for a given Shipment Stage. (epo-ful:ShipmentStage -> epo-ful:ShipmentInformation (+epo:hasArrivalShipmentInformation))"@en ;
	skos:prefLabel "Has arrival shipment information"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasAssociatedDocument a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Is related to a Document. (epo-ful:DespatchLine -> epo:Document (+epo-ful:hasAssociatedDocument)) Is related to a Document. (epo-ful:DespatchAdvice -> epo:Document (+epo-ful:hasAssociatedDocument))"@en ;
	skos:prefLabel "Has associated document"@en ;
	skos:historyNote "WG Approval 06/10/2024"@en .

a4g:hasCarrierConsignmentID a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The identifier of the the Consignment given by the Carrier. (epo-ful:Consignment -> adms:Identifier (+epo-ful:hasCarrierConsignmentID))"@en ;
	skos:prefLabel "Has carrier consignment i d"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:hasCarrierServiceInstruction a owl:DatatypeProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "This attribute should be removed in the next major release. (epo-ful:Consignment)"@en ;
	skos:prefLabel "Has carrier service instruction"@en .

a4g:hasChargeableWeight a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The (epo-ful:Consignment -> epo:Quantity (+epo-ful:hasChargeableWeight))"@en ;
	skos:prefLabel "Has chargeable weight"@en .

a4g:hasConsigneeConsignmentID a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The identifier of the the Consignment given by the Consignee. (epo-ful:Consignment -> adms:Identifier (+epo-ful:hasConsigneeConsignmentID))"@en ;
	skos:prefLabel "Has consignee consignment i d"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:hasConsignmentDeclaredStatisticsValue a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:prefLabel "Has consignment declared statistics value"@en .

a4g:hasConsignmentFreeOnBoardValue a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The Monetary Value of the Consignment when there is a transfer of liability for goods, damaged or destroyed, during shipping. at the exporter's customs frontier. (epo-ful:Consignment -> epo:MonetaryValue (+epo-ful:hasConsignmentFreeOnBoardValue))"@en ;
	skos:prefLabel "Has consignment free on board value"@en .

a4g:hasConsignmentInvoiceValue a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Declared amount of the Consignment. Additional Information: This is a logistics information needed for logistics, security measures, customs check, etc. (epo-ful:Consignment -> epo:MonetaryValue (+epo-ful:hasConsignmentInvoiceValue))"@en ;
	skos:prefLabel "Has consignment invoice value"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasDeclaredStatisticalValue a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The Monetary Value declared for statistical purposes. (epo-ful:Consignment -> epo:MonetaryValue (+epo-ful:hasDeclaredStatisticalValue))"@en ;
	skos:prefLabel "Has declared statistical value"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:hasDeliveryInstruction a owl:DatatypeProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "This attribute should be removed in the next major release. (epo-ful:Consignment)"@en ;
	skos:prefLabel "Has delivery instruction"@en .

a4g:hasDepartureShipmentInformation a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Shipment Information about the place the Consignment is leaving from in a given Shipment Stage. (epo-ful:ShipmentStage -> epo-ful:ShipmentInformation (+epo-ful:hasDepartureShipmentInformation))"@en ;
	skos:prefLabel "Has departure shipment information"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasDespatchAdviceType a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Classification of a Despatch Advice. (epo-ful:DespatchAdvice -> at-voc-new:despatch-advice-type (+epo-ful:hasDespatchAdviceType))"@en ;
	skos:prefLabel "Has despatch advice type"@en .

a4g:hasDespatchPeriod a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The time period when the Despatcher picks-up the Goods Items to be delivered. (epo-ful:ShipmentInformation -> epo:Period (+epo-ful:hasDespatchPeriod))"@en ;
	skos:prefLabel "Has despatch period"@en ;
	skos:historyNote "WG Approval 16/07/2024"@en .

a4g:hasDespatchedQuantity a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The Quantity that has been sent. (epo-ful:DespatchLine -> epo:Quantity (+epo-ful:hasDespatchedQuantity))"@en ;
	skos:prefLabel "Has despatched quantity"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasDespatcherConsignmentID a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The identifier of the the Consignment given by the Despatcher. (epo-ful:Consignment -> adms:Identifier (+epo-ful:hasDespatcherConsignmentID))"@en ;
	skos:prefLabel "Has despatcher consignment i d"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:hasDocumentStatus a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "https://test-docs.peppol.eu/logistics/transport-execution/codelist/DocumentStatusCode/ (epo-cat:PostAwardDocument -> at-voc-new:document-status (+epo:hasDocumentStatus))"@en ;
	skos:prefLabel "Has document status"@en .

a4g:hasDriverPerson a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Relation indicating the operator of the Transport Means. (epo-ful:TransportMeans -> epo-ful:TransportMeansOperator (+epo:hasDriverPerson))"@en ;
	skos:prefLabel "Has driver person"@en ;
	skos:historyNote "WG Approval 02/07/2024"@en .

a4g:hasEstimatedDeliveryPeriod a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The forecasted time period given by the Despatcher to provide the goods and services. (epo-ful:ShipmentInformation -> epo:Period (+epo-ful:hasEstimatedDeliveryPeriod))"@en ;
	skos:prefLabel "Has estimated delivery period"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasFreightAllowanceCharge a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:prefLabel "Has freight allowance charge"@en .

a4g:hasFreightCharge a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The total cost for moving the goods. (epo-ful:Consignment -> epo-cat:ChargeInformation (+epo-ful:hasFreightCharge))"@en ;
	skos:prefLabel "Has freight charge"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasFreightForwarderConsignmentID a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The identifier of the the Consignment given by the Freight Forwarder. (epo-ful:Consignment -> adms:Identifier (+epo-ful:hasFreightForwarderConsignmentID))"@en ;
	skos:prefLabel "Has freight forwarder consignment i d"@en ;
	skos:historyNote "WG Approval 10/06/2024"@en .

a4g:hasGrossVolume a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The amount of space that is required including the packaging. (epo-ful:AbstractContainer -> epo:Quantity (+epo-ful:hasGrossVolume)) The amount of space that is required including the packaging. (epo-ful:Consignment -> epo:Quantity (+epo-ful:hasGrossVolume))"@en ;
	skos:prefLabel "Has gross volume"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasGrossWeight a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The total mass of goods including the packaging. (epo-ful:AbstractContainer -> epo:Quantity (+epo-ful:hasGrossWeight)) The total mass of goods including the packaging. (epo-ful:Consignment -> epo:Quantity (+epo-ful:hasGrossWeight))"@en ;
	skos:prefLabel "Has gross weight"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasHandlingInstruction a owl:DatatypeProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Instruction on how to handle the Goods Items during transportation. (epo-ful:Consignment) Instruction on how to handle the Goods Items during transportation. (epo-ful:TransportHandlingUnit)"@en ;
	skos:prefLabel "Has handling instruction"@en ;
	skos:historyNote "WG Approval 05/06/2024"@en .

a4g:hasHandlingInstructionCode a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "Instruction on how to handle the Goods Items during transportation, expressed as a code. (epo-ful:Consignment -> at-voc-new:handling-instruction (+epo-ful:hasHandlingInstructionCode)) Instruction on how to handle the Goods Items during transportation, expressed as a code. (epo-ful:TransportHandlingUnit -> at-voc-new:handling-instruction (+epo-ful:hasHandlingInstructionCode))"@en ;
	skos:prefLabel "Has handling instruction code"@en .

a4g:hasHeight a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The vertical measurement. (epo-ful:AbstractContainer -> epo:Quantity (+epo-ful:hasHeight))"@en ;
	skos:prefLabel "Has height"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasLength a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The horizontal measurement. Additional Information: The length is the longer side between the length and the width. (epo-ful:AbstractContainer -> epo:Quantity (+epo-ful:hasLength))"@en ;
	skos:prefLabel "Has length"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasLoadingLength a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The length (epo-ful:Consignment -> epo:Quantity (+epo-ful:hasLoadingLength))"@en ;
	skos:prefLabel "Has loading length"@en .

a4g:hasLoadingMeter a owl:ObjectProperty ;
	rdfs:isDefinedBy a4g:ful ;
	skos:definition "The area of the Consignment in meters divided by the width of the truck, which is considered to be 2.4 meters. (epo-ful:Consignment -> epo:Quantity (+epo-ful:hasLoadingMeter))"@en ;
	skos:prefLabel "Has loading meter"@en ;
	skos:historyNote "WG Approval 18/06/2024"@en .

a4g:hasMainCarriageShipmentStage a owl:ObjectProperty ;
